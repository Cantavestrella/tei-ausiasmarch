 <text>
  <body>
   <div type="poem" xml:id="C025-K025">
    <pb n="26r" facs="http://www.cervantesvirtual.com/obra-visor/les-obres-de-mossen-ausias-march-sont-acabada-de-scriure-la-present-obra-per-m-pere-vilaraso-prevere-per-manament-de-lilltre-sor-almirant-de-napols-manuscrit--0/html/00488f44-82b2-11df-acc7-002185ce6064_58.html"/>
    <lg type="cobla" n="1" xml:id="C025-lg1">
     <l n="1" xml:id="C025-l1"> n Om fall recort del temps <choice>
      <abbr>tȃt</abbr>
       <expan>tant</expan>
      </choice>
      <choice>
       <abbr>ƌlitos</abbr>
       <expan>delitos</expan>
      </choice>
     </l>
     <l n="2" xml:id="C025-l2"> Ques ya passat pens <choice>
      <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice> tal no vendra </l>
     <l n="3" xml:id="C025-l3"> Sil conseguesch merçe nom fallira </l>
     <l n="4" xml:id="C025-l4"> Car piatat fetaura pau ab vos </l>
     <l n="5" xml:id="C025-l5"> Preareumi quin <choice>
      <abbr>ŧps</abbr>
       <expan>temps</expan>
      </choice> antich preaueu </l>
     <l n="6" xml:id="C025-l6"> E confessant queus dolieu de mj </l>
     <l n="7" xml:id="C025-l7"> Ara queus am pus <choice>
      <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice> james amj </l>
     <l n="8" xml:id="C025-l8"> Tornau vos lla hon deprimer estaueu </l>
    </lg>
    <lg type="cobla" n="2" xml:id="C025-lg2">
     <l n="9" xml:id="C025-l9"> <c rend="large">P</c>usque dabans me trobe desijos </l>
     <l n="10" xml:id="C025-l10"> E lo desitx jamens en mis morra </l>
     <l n="11" xml:id="C025-l11"> Car per sa part mon cos la <choice>
      <abbr>sostȇdra</abbr>
       <expan>sostendra</expan>
      </choice>
     </l>
     <l n="12" xml:id="C025-l12"> Lenteniment no sera despitos </l>
     <l n="13" xml:id="C025-l13"> Vos desijau amj qui desijaveu </l>
     <l n="14" xml:id="C025-l14"> Per tal voler del qual jom <choice>
      <abbr>cȏtenti</abbr>
       <expan>contenti</expan>
      </choice>
     </l>
     <l n="15" xml:id="C025-l15"> Ara queus am pus <choice>
      <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice> james amj </l>
     <l n="16" xml:id="C025-l16"> Tornau uos lla hon deprimer staueu </l>
    </lg>
    <lg type="cobla" n="3" xml:id="C025-lg3">
     <l n="17" xml:id="C025-l17"> <c rend="large">E</c> si raho fon <choice>
      <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice> ben volgut fos </l>
     <l n="18" xml:id="C025-l18"> Mils ho meresch mon vll <choice>
      <abbr>nȏ</abbr>
       <expan>nom</expan>
      </choice>
      <choice>
       <abbr>desmȇtra</abbr>
       <expan>desmentra</expan>
      </choice>
     </l>
     <l n="19" xml:id="C025-l19"> Car per gran dol moltes veus ne plora </l>
     <l n="20" xml:id="C025-l20"> E no plorant <choice>
      <abbr>mostrauȃ</abbr>
       <expan>mostrauam</expan>
      </choice> doloros </l>
     <l n="21" xml:id="C025-l21"> E vos de gotx lo Te <choice>
      <abbr>deȗ</abbr>
       <expan>deum</expan>
      </choice> cantaueu </l>
     <l n="22" xml:id="C025-l22"> Lagrimeiat malediccions canti </l>
     <l n="23" xml:id="C025-l23"> Ara queus am pus <choice>
      <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice> james amj </l>
     <l n="24" xml:id="C025-l24"> Tornau uos <choice>
      <abbr>llȃ</abbr>
       <expan>llan</expan>
      </choice> hon <choice>
       <abbr>ƌ</abbr>
       <expan>de</expan>
      </choice> primer staueu </l>
    </lg>
    <pb n="26v" facs="http://www.cervantesvirtual.com/obra-visor/les-obres-de-mossen-ausias-march-sont-acabada-de-scriure-la-present-obra-per-m-pere-vilaraso-prevere-per-manament-de-lilltre-sor-almirant-de-napols-manuscrit--0/html/00488f44-82b2-11df-acc7-002185ce6064_59.html"/>
    <lg type="cobla" n="4" xml:id="C025-lg4">
     <l n="25" xml:id="C025-l25"> <c rend="large">C</c>omptar no dech les passades dolos </l>
     <l n="26" xml:id="C025-l26"> Car poca fe vos tost hi sera </l>
     <l n="27" xml:id="C025-l27"> Quis menys damor altramor no sentra </l>
     <l n="28" xml:id="C025-l28"> Car no hasguart lom rient al ploros </l>
     <l n="29" xml:id="C025-l29"> Si ben amant lo terç <choice>
      <abbr>dȗ</abbr>
       <expan>dun</expan>
      </choice> jorn passaueu </l>
     <l n="30" xml:id="C025-l30"> Haurieu grat del <choice>
      <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice> per vos passi </l>
     <l n="31" xml:id="C025-l31"> Ara <choice>
      <abbr>ꝗus</abbr>
       <expan>queus</expan>
      </choice> am pus <choice>
       <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice> james amj </l>
     <l n="32" xml:id="C025-l32"> Tornau uos lla hon de primer staueu </l>
    </lg>
    <lg type="cobla" n="5" xml:id="C025-lg5">
     <l n="33" xml:id="C025-l33"> <c rend="large">R</c>eclam atots los meus predeçessos </l>
     <l n="34" xml:id="C025-l34"> Cells qui amor llur cor enamora </l>
     <l n="35" xml:id="C025-l35"> E los presents elo qui naxera </l>
     <l n="36" xml:id="C025-l36"> Que per mos dits <choice>
      <abbr>entenguȇ</abbr>
       <expan>entenguen</expan>
      </choice> mos clamos </l>
     <l n="37" xml:id="C025-l37"> E si enuos conexença justaueu </l>
     <l n="38" xml:id="C025-l38"> Mal grat haureu del <choice>
      <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice> fes un matj </l>
     <l n="39" xml:id="C025-l39"> Ara queus am pus <choice>
      <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice> james amj </l>
     <l n="40" xml:id="C025-l40"> Tornau uos llan hon de primer staueu </l>
    </lg>
    <lg type="tornada" n="6" xml:id="C025-lg6">
     <l n="41" xml:id="C025-l41"> <c rend="large">P</c>lena de seny sil cor me carteiaueu </l>
     <l n="42" xml:id="C025-l42"> Trobareu clar <choice>
      <abbr>queꝯ</abbr>
       <expan>queus</expan>
      </choice> amare sens fi </l>
     <l n="43" xml:id="C025-l43"> Ara queus am pus <choice>
      <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice> james amj </l>
     <l n="44" xml:id="C025-l44"> Tornau uos lla hon de primer estaueu </l>
    </lg>
   </div>
  </body>
 </text>
