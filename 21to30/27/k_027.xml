 <text>
  <body>
   <div type="poem" xml:id="C027-K027">
    <pb n="28r" facs="http://www.cervantesvirtual.com/obra-visor/les-obres-de-mossen-ausias-march-sont-acabada-de-scriure-la-present-obra-per-m-pere-vilaraso-prevere-per-manament-de-lilltre-sor-almirant-de-napols-manuscrit--0/html/00488f44-82b2-11df-acc7-002185ce6064_62.html"/>
    <lg type="cobla" n="1" xml:id="C027-lg1">
     <l n="1" xml:id="C027-l1"> s Obres dolor ma tolt limaginar </l>
     <l n="2" xml:id="C027-l2"> Lenteniment nos dol nes pot sbatre </l>
     <l n="3" xml:id="C027-l3"> Ay tant es dolt <choice>
      <abbr>ꝗla</abbr>
       <expan>quela</expan>
      </choice> calgut abatre </l>
     <l n="4" xml:id="C027-l4"> E <choice>
      <abbr>mȏ</abbr>
       <expan>mon</expan>
      </choice> affany plorant nos pot mostrar </l>
     <l n="5" xml:id="C027-l5"> No trop remey car ma dolor es tanta </l>
     <l n="6" xml:id="C027-l6"> Que <choice>
      <abbr>mȏ</abbr>
       <expan>mon</expan>
      </choice> voler en parts ne tinch partit </l>
     <l n="7" xml:id="C027-l7"> Nen sols vn lloch lom trobe ahunjt </l>
     <l n="8" xml:id="C027-l8"> Si mon morir e viure ques <choice>
      <abbr>decȃta</abbr>
       <expan>decanta</expan>
      </choice>
     </l>
    </lg>
    <lg type="cobla" n="2" xml:id="C027-lg2">
     <l n="9" xml:id="C027-l9"> <c rend="large">S</c>i com lom flach quil es forçat triar </l>
     <l n="10" xml:id="C027-l10"> Ab qual de dos homens forts sa <choice>
      <abbr>cṁbatre</abbr>
       <expan>combatre</expan>
      </choice>
     </l>
     <l n="11" xml:id="C027-l11"> No sab <choice>
      <abbr>pȇsant</abbr>
       <expan>pensant</expan>
      </choice> ab qual dega debatre </l>
     <l n="12" xml:id="C027-l12"> Spaordit sos comptes no pot far </l>
     <l n="13" xml:id="C027-l13"> Ne pren amj <choice>
      <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice> lo viurem spanta </l>
     <l n="14" xml:id="C027-l14"> E lo morir me sera <choice>
       <abbr>gra</abbr>
       <expan>gran</expan>
      </choice> despit </l>
     <l n="15" xml:id="C027-l15"> Com viure vul la mort <choice>
      <abbr>prȇc</abbr>
       <expan>prenc</expan>
      </choice> en <choice>
       <abbr>grȃ</abbr>
       <expan>gran</expan>
      </choice>
      <choice>
       <abbr>ƌlit</abbr>
       <expan>delit</expan>
      </choice>
     </l>
     <l n="16" xml:id="C027-l16"> Com vull morir la vida tinch <choice>
       <abbr>p</abbr>
       <expan>per</expan>
      </choice> santa </l>
    </lg>
    <pb n="28v" facs="http://www.cervantesvirtual.com/obra-visor/les-obres-de-mossen-ausias-march-sont-acabada-de-scriure-la-present-obra-per-m-pere-vilaraso-prevere-per-manament-de-lilltre-sor-almirant-de-napols-manuscrit--0/html/00488f44-82b2-11df-acc7-002185ce6064_63.html"/>
    <lg type="cobla" n="3" xml:id="C027-lg3">
     <l n="17" xml:id="C027-l17"> <c rend="large">S</c>i <choice>
      <abbr>cṁ</abbr>
       <expan>com</expan>
      </choice>
      <choice>
       <abbr>aꝗll</abbr>
       <expan>aquell</expan>
      </choice> qui es veri <choice>
       <abbr>donȃt</abbr>
       <expan>donant</expan>
      </choice>
     </l>
     <l n="18" xml:id="C027-l18"> Al mestre seu e <choice>
      <abbr>quȃt</abbr>
       <expan>quant</expan>
      </choice> veu sa dolor </l>
     <l n="19" xml:id="C027-l19"> Ha pietat <choice>
      <abbr>ƌl</abbr>
       <expan>del</expan>
      </choice> mal de son senyor </l>
     <l n="20" xml:id="C027-l20"> E <choice>
      <abbr>sobraꝗll</abbr>
       <expan>sobraquell</expan>
      </choice> vol esser aiudant </l>
     <l n="21" xml:id="C027-l21"> Vos pensament per qui <choice>
      <abbr>mȏ</abbr>
       <expan>mon</expan>
      </choice> seny <choice>
       <abbr>sabsȇta</abbr>
       <expan>sabsenta</expan>
      </choice>
     </l>
     <l n="22" xml:id="C027-l22"> Per los treballs dantli alterament </l>
     <l n="23" xml:id="C027-l23"> Ason affany donan acorriment </l>
     <l n="24" xml:id="C027-l24"> Puys <choice>
      <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice> sos mals per mija vostrals senta </l>
    </lg>
    <lg type="cobla" n="4" xml:id="C027-lg4">
     <l n="25" xml:id="C027-l25"> <c rend="large">Y</c>o contrafas nau en golf perillan </l>
     <l n="26" xml:id="C027-l26"> Larbre perdent e son gouernador </l>
     <l n="27" xml:id="C027-l27"> E <choice>
      <abbr>ꝑ</abbr>
       <expan>per</expan>
      </choice>
      <choice>
       <abbr>cȏtrast</abbr>
       <expan>contrast</expan>
      </choice>
      <choice>
       <abbr>ƌ</abbr>
       <expan>de</expan>
      </choice> dos vents no discorr </l>
     <l n="28" xml:id="C027-l28"> Los marines enbadalits <choice>
       <abbr>esta</abbr>
       <expan>estan</expan>
      </choice>
     </l>
     <l n="29" xml:id="C027-l29"> E cascun dells la sua carta <choice>
      <abbr>tȇta</abbr>
       <expan>tenta</expan>
      </choice>
     </l>
     <l n="30" xml:id="C027-l30"> E son discorts en lur <choice>
      <abbr>acordamȇt</abbr>
       <expan>acordament</expan>
      </choice>
     </l>
     <l n="31" xml:id="C027-l31"> Hu volgra ser prop terra passos <choice>
       <abbr>cêt</abbr>
       <expan>cent</expan>
      </choice>
     </l>
     <l n="32" xml:id="C027-l32"> Laltre tan <choice>
       <abbr>luñ</abbr>
       <expan>luny</expan>
      </choice> com vent pot dar <choice>
       <abbr>empȇta</abbr>
       <expan>empenta</expan>
      </choice>
     </l>
    </lg>
    <lg type="cobla" n="5" xml:id="C027-lg5">
     <l n="33" xml:id="C027-l33"> <c rend="large">M</c>a <choice>
      <abbr>volȗtat</abbr>
       <expan>voluntat</expan>
      </choice> ab quen lamor fuy <choice>
       <abbr>mês</abbr>
       <expan>mes</expan>
      </choice>
     </l>
     <l n="34" xml:id="C027-l34"> Fallida es e pogram fer aiuda </l>
     <l n="35" xml:id="C027-l35"> Ya ma raho de son loch es cayguda </l>
     <l n="36" xml:id="C027-l36"> Mos <choice>
      <abbr>pȇsaments</abbr>
       <expan>pensaments</expan>
      </choice> contraris man ates </l>
     <l n="37" xml:id="C027-l37"> Ya mos desitxs no <choice>
      <abbr>sabȇ</abbr>
       <expan>saben</expan>
      </choice> elegir </l>
     <l n="38" xml:id="C027-l38"> Vida ne mort qual es la mjllor tria </l>
     <l n="39" xml:id="C027-l39">
      <choice>
       <abbr>Naturȃ</abbr>
       <expan>Naturan</expan>
      </choice> mi vsa de maestria </l>
     <l n="40" xml:id="C027-l40"> E pren la mort per maior <choice>
      <abbr>dȃ</abbr>
       <expan>dan</expan>
      </choice> fugir </l>
    </lg>
    <lg type="tornada" n="6" xml:id="C027-lg6">
     <l n="41" xml:id="C027-l41"> <c rend="large">P</c>lena de seny nos pot ben soferir </l>
     <l n="42" xml:id="C027-l42"> Viay dolor sens <choice>
      <abbr>pendralgȗ</abbr>
       <expan>pendralgun</expan>
      </choice> spay </l>
     <pb n="29r" facs="http://www.cervantesvirtual.com/obra-visor/les-obres-de-mossen-ausias-march-sont-acabada-de-scriure-la-present-obra-per-m-pere-vilaraso-prevere-per-manament-de-lilltre-sor-almirant-de-napols-manuscrit--0/html/00488f44-82b2-11df-acc7-002185ce6064_64.html"/>
     <l n="43" xml:id="C027-l43"> Quant <choice>
      <abbr>ṁ</abbr>
       <expan>me</expan>
      </choice> recort <choice>
       <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice> res vos aia dir </l>
    </lg>
   </div>
  </body>
 </text>
