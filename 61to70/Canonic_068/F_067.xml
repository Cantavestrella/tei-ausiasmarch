   <text>
      <body>
         <div type="poem" xml:id="C068-F067">

            <pb n="67v" facs="https://www.cervantesvirtual.com/obra-visor/poesias-de-ausias-y-mena-manuscrito--0/html/0165fe84-82b2-11df-acc7-002185ce6064_138.html"/>

            <lg type="cobla" n="1" xml:id="C068-lg1">

               <l n="1" xml:id="C068-l1"> <c rend="large">N</c>om pren axi com al petit vaylet </l>

               <l n="2" xml:id="C068-l2"> Qui ua cerquant senyor quj festal faca </l>

               <l n="3" xml:id="C068-l3"> Tenint lo calt en lo temps dela glaca </l>

               <l n="4" xml:id="C068-l4"> E fresch destiu com la calor se met </l>

               <l n="5" xml:id="C068-l5"> Preant molt poch la ualor del senyor </l>

               <l n="6" xml:id="C068-l6"> E concebent de salt de samanera </l>

               <l n="7" xml:id="C068-l7"> Vehent molt clar que te mala carrera </l>

               <l n="8" xml:id="C068-l8"> De cambiar son estat en major </l>

            </lg>

            <lg type="cobla" n="2" xml:id="C068-lg3">

               <l n="9" xml:id="C068-l17"> <c rend="large">Y</c>o son aquell qujn lo temps de tempesta </l>

               <l n="10" xml:id="C068-l18"> Quant les mes gents festegen prop los fochs </l>

               <l n="11" xml:id="C068-l19"> E pusch hauer ab ells los proprjs jochs </l>

               <l n="12" xml:id="C068-l20"> Vaig sobre neu descalc ab nua testa </l>

               <l n="13" xml:id="C068-l21"> Serujnt senyor quj james fon vassall </l>

               <l n="14" xml:id="C068-l22"> Nel vench esment de fer may homenatge </l>

               <l n="15" xml:id="C068-l23"> En tot leig fet hague lo cor saluatge </l>

               <l n="16" xml:id="C068-l24"> Solament diu que bon guardo nom fall </l>

            </lg>

            <pb n="68r" facs="https://www.cervantesvirtual.com/obra-visor/poesias-de-ausias-y-mena-manuscrito--0/html/0165fe84-82b2-11df-acc7-002185ce6064_139.html"/>

            <lg type="tornada" n="3" xml:id="C068-lg4">

               <l n="17" xml:id="C068-l25"> <c rend="large">P</c>lena de seny leigs desigs de mj tall </l>

               <l n="18" xml:id="C068-l26"> Erbes nos fan males en mon ribatge </l>

               <l n="19" xml:id="C068-l27"> Sia entes com dins en mon coratge </l>

               <l n="20" xml:id="C068-l28"> Los penssaments nom deuallen auall </l>

            </lg>

         </div>
      </body>
