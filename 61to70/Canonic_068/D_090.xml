   <text>
      <body>
         <div type="poem" xml:id="C068-D090">

            <pb n="95v" facs="https://www.cervantesvirtual.com/obra/poesies-3/"/>

            <lg type="cobla" n="1" xml:id="C068-lg1">

               <l n="1" xml:id="C068-l1">
                  <c rend="large">N</c>ompren axi / com al petit vaylet </l>

               <l n="2" xml:id="C068-l2"> quiva cercant / <choice>
                     <abbr>Senōr</abbr>
                     <expan>Senyor</expan>
                  </choice> qui festalfaça </l>

               <l n="3" xml:id="C068-l3"> tenint lo calt / en lo temps delaglaça </l>

               <l n="4" xml:id="C068-l4"> efresch destiu / comlacalor se met </l>

               <l n="5" xml:id="C068-l5"> Preant molt poch / la valor del senyor </l>

               <l n="6" xml:id="C068-l6"> econcebent / de<del rend="overwritten">f</del><add
                     place="superimposed">sh</add>alt de sa manera </l>

               <l n="7" xml:id="C068-l7"> vehent molt clar / que te mala carrera </l>

               <l n="8" xml:id="C068-l8"> decambiar / son estat en maior </l>

            </lg>

            <lg type="cobla" n="2" xml:id="C068-lg3">

               <l n="9" xml:id="C068-l17">
                  <c rend="large">Y</c>oson aquell / <del rend="overstrike">quilo</del><add
                     place="interlinear">qui'nlo<note type="editorial">D'una altra mà.</note></add>
                  temps detempesta </l>

               <l n="10" xml:id="C068-l18"> quant les mes gents / festejen proplos fochs </l>

               <l n="11" xml:id="C068-l19">
                  <add>h</add>e puch hauer / ab ells los <choice>
                     <abbr>ṗprisjochs</abbr>
                     <expan>proprisjochs</expan>
                  </choice>
               </l>

               <l n="12" xml:id="C068-l20"> vaig sobre neu / descalç ab <del rend="overstrike"
                     >una</del><add place="interlinear">nua</add> testa </l>

               <l n="13" xml:id="C068-l21"> Seruint <choice>
                  <abbr>senōr</abbr>
                     <expan>senyor</expan>
                  </choice> / qui james fon vassall </l>

               <l n="14" xml:id="C068-l22"> nel vench esment / de fer may homenatje </l>

               <l n="15" xml:id="C068-l23">
                  <choice>
                     <abbr>ē</abbr>
                     <expan>en</expan>
                  </choice>tot leig fet / hague lo cor saluatje </l>

               <l n="16" xml:id="C068-l24"> solament diu / quebonguardo nom fall </l>

            </lg>

            <lg type="tornada" n="3" xml:id="C068-lg4">

               <l n="17" xml:id="C068-l25">
                  <c rend="large">P</c>lena de seny / leigs desigs de mi tall </l>

               <l n="18" xml:id="C068-l26"> erbes nos fan / males en mon ribatge </l>

               <l n="19" xml:id="C068-l27"> sia entes / com dins en moncoratje </l>

               <l n="20" xml:id="C068-l28"> los pensaments / nom deuallen <del rend="overstrike"
                     >amal</del><add>auall</add>
               </l>

            </lg>

         </div>
      </body>
