   <text>
      <body>
         <div type="poem" xml:id="C064-N063">

            <pb n="132" facs="https://www.cervantesvirtual.com/portales/ausias_march/obra-visor/canconer-de-obres-de-mossen-ausias-march-manuscrito--0/html/017bdf6a-82b2-11df-acc7-002185ce6064_134.html"/>

            <lg type="cobla" n="1" xml:id="C064-lg1">

               <l n="1" xml:id="C064-l1"> ¶ Lo temps es tal que tot animal brut </l>

               <l n="2" xml:id="C064-l2"> Requer amor quiscu trobant son par </l>

               <l n="3" xml:id="C064-l3"> Lo ceruo brau sen enlo bosch bramar </l>

               <l n="4" xml:id="C064-l4"> E son fer bram per dolc cant es tengut </l>

               <l n="5" xml:id="C064-l5"> Agrons e corps han melodia tanta </l>

               <l n="6" xml:id="C064-l6"> Que lur semblant delitant enamora </l>

               <l n="7" xml:id="C064-l7"> Lo rosiyol de tals cas sentrenyora </l>

               <l n="8" xml:id="C064-l8"> Si lo seu cant sanamoradaspanta </l>

            </lg>

            <lg type="cobla" n="2" xml:id="C064-lg2">

               <l n="9" xml:id="C064-l9"> E donchs sim dolch lo dolremes degut </l>

               <l n="10" xml:id="C064-l10"> Com veig amats meyns de poder amar </l>

               <l n="11" xml:id="C064-l11"> E lo grosser per apte veg passar </l>

               <l n="12" xml:id="C064-l12"> Amor lo fa esser no conegut </l>

               <l n="13" xml:id="C064-l13"> E dacomve piadossa complanta </l>

               <l n="14" xml:id="C064-l14"> Com desamor exorba ma senyora </l>

               <l n="15" xml:id="C064-l15"> No conexent lo seruent quj ladora </l>

               <l n="16" xml:id="C064-l16"> Ne vol pensar qual es samor e quanta </l>

            </lg>

            <lg type="cobla" n="3" xml:id="C064-lg3">

               <l n="17" xml:id="C064-l17"> No com aquell quj son be aperdut </l>

               <l n="18" xml:id="C064-l18"> Metent arisch siporia guanyar </l>

               <l n="19" xml:id="C064-l19"> Evos amat quem volguesseu amar </l>

               <l n="20" xml:id="C064-l20"> Desliberat nosonamor vengut </l>

               <l n="21" xml:id="C064-l21"> Tot nuu me trob vestit de grossa manta </l>

               <l n="22" xml:id="C064-l22"> Ma voluntat amor latenpenyora </l>

               <l n="23" xml:id="C064-l23"> E co deque mon cor se adolora </l>

               <l n="24" xml:id="C064-l24"> Es com no veu ma fretura ques tanta </l>

            </lg>

            <pb n="133" facs="https://www.cervantesvirtual.com/portales/ausias_march/obra-visor/canconer-de-obres-de-mossen-ausias-march-manuscrito--0/html/017bdf6a-82b2-11df-acc7-002185ce6064_135.html"/>

            <lg type="tornada" n="4" xml:id="C064-lg4">

               <l n="25" xml:id="C064-l25"> Lir entre carts abmilans cac laganta </l>

               <l n="26" xml:id="C064-l26"> Y ablo branget la lebra corredora </l>

               <l n="27" xml:id="C064-l27"> Asats almon quiscunas viuidora </l>

               <l n="28" xml:id="C064-l28"> E mon pits flach la pasi derams canta </l>

            </lg>

         </div>
      </body>
   </text>
