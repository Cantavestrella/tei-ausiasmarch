   <text>
      <body>
         <div type="poem" xml:id="C064-H070">

            <pb n="86v" facs="https://www.cervantesvirtual.com/obra-visor/poetas-catalanes-manuscrito-biblioteca-de-la-universidad-de-zaragoza-manuscrit--0/html/01660e74-82b2-11df-acc7-002185ce6064_155.html"/>

            <lg type="cobla" n="1" xml:id="C064-lg1">

               <l n="1" xml:id="C064-l1"> <c rend="large">L</c>o temps es tall que tot anjmall brut </l>

               <l n="2" xml:id="C064-l2"> Requer amor cascu trobant son par </l>

               <l n="3" xml:id="C064-l3"> Lo seruo brau sent en lo bosch bramar </l>

               <l n="4" xml:id="C064-l4"> E son ffer bram per dolç cant es tengut </l>

               <l n="5" xml:id="C064-l5"> Agrons e corps / han melodia tanta </l>

               <l n="6" xml:id="C064-l6"> Que llur semblant delitant enemora </l>

               <l n="7" xml:id="C064-l7"> Lo rossinyoll / de tal cars sentrenyora </l>

               <l n="8" xml:id="C064-l8"> Si lo seu cant sanamoradaspanta </l>

            </lg>

            <lg type="cobla" n="2" xml:id="C064-lg2">

               <l n="9" xml:id="C064-l9"> E donchs sim dolch lo dolrem es degut </l>

               <l n="10" xml:id="C064-l10"> Com veig amats menys <choice>
                  <abbr>đ</abbr>
                     <expan>de</expan>
                  </choice> poder amar </l>

               <l n="11" xml:id="C064-l11"> E lo grosser per abta veig passar </l>

               <l n="12" xml:id="C064-l12"> Amor lo fa esser no conagut </l>

               <l n="13" xml:id="C064-l13"> E daçom ue piadosa complanta </l>

               <l n="14" xml:id="C064-l14"> Com desamor exorpa ma senyora </l>

               <l n="15" xml:id="C064-l15"> No conaxent lo seruent <choice>
                  <abbr>ꝙ</abbr>
                     <expan>qui</expan>
                  </choice> ladora </l>

               <l n="16" xml:id="C064-l16"> Ne vol pensar qual es samor ne quanta </l>

            </lg>

            <lg type="cobla" n="3" xml:id="C064-lg3">

               <l n="17" xml:id="C064-l17"> No com aquell <choice>
                  <abbr>ꝙ</abbr>
                     <expan>qui</expan>
                  </choice> son be ha perdut </l>

               <l n="18" xml:id="C064-l18"> Matent arisch si poria guanyar </l>

               <l n="19" xml:id="C064-l19"> E uos amat quem volguesseu amar </l>

               <l n="20" xml:id="C064-l20"> Delliberat nossonamor vengut </l>

               <l n="21" xml:id="C064-l21"> Tot nuu me trob vestit <choice>
                  <abbr>đ</abbr>
                     <expan>de</expan>
                  </choice> grossa manta </l>

               <l n="22" xml:id="C064-l22"> Ma voluntat amor la tenpenyora </l>

               <l n="23" xml:id="C064-l23"> E ço <choice>
                  <abbr>đ</abbr>
                     <expan>de</expan>
                  </choice> que mon cor se adolora </l>

               <l n="24" xml:id="C064-l24"> Es com no veu ma fretura ques tanta </l>

            </lg>

            <lg type="tornada" n="4" xml:id="C064-lg4">

               <l n="25" xml:id="C064-l25"> Lir entre carts ab mjlans caç la ganta </l>

               <l n="26" xml:id="C064-l26"> Y ab lo branxet la lebra corradora </l>

               <l n="27" xml:id="C064-l27"> Assats al mon cascunas vjujdora </l>

               <l n="28" xml:id="C064-l28"> E mon pits flach lo passi <choice>
                  <abbr>đ</abbr>
                     <expan>de</expan>
                  </choice> rams canta </l>

            </lg>

         </div>
