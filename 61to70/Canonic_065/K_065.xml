 <text>
  <body>
   <div type="poem" xml:id="C065-K065">

    <pb n="63r"
     facs="https://www.cervantesvirtual.com/obra-visor/les-obres-de-mossen-ausias-march-sont-acabada-de-scriure-la-present-obra-per-m-pere-vilaraso-prevere-per-manament-de-lilltre-sor-almirant-de-napols-manuscrit--0/html/00488f44-82b2-11df-acc7-002185ce6064_132.html"/>

    <lg type="cobla" n="1" xml:id="C065-lg1">

     <l n="1" xml:id="C065-l1"> n O so gosat en demanar merce </l>

     <l n="2" xml:id="C065-l2"> A ma dolor <choice>
       <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice> del tot mabandon </l>

     <l n="3" xml:id="C065-l3"> Vn poch spay de temps la prech <choice>
       <abbr>ꝗm</abbr>
       <expan>quem</expan>
      </choice> don </l>

     <l n="4" xml:id="C065-l4"> Apoder dir lo mal <choice>
       <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice>
      <choice>
       <abbr>dellā</abbr>
       <expan>dellam</expan>
      </choice> ve </l>

     <l n="5" xml:id="C065-l5"> E si <choice>
       <abbr>mō</abbr>
       <expan>mon</expan>
      </choice> cor en sa força retorn </l>

     <l n="6" xml:id="C065-l6"> Y el <choice>
       <abbr>torbamēt</abbr>
       <expan>torbament</expan>
      </choice>
      <choice>
       <abbr>đl</abbr>
       <expan>del</expan>
      </choice>
      <choice>
       <abbr>entenimēt</abbr>
       <expan>enteniment</expan>
      </choice> pert </l>

     <l n="7" xml:id="C065-l7"> Pora saber qui damor no es cert </l>

     <l n="8" xml:id="C065-l8"> Yra y amor com dins mj an <choice>
       <abbr>cōtorn</abbr>
       <expan>contorn</expan>
      </choice>
     </l>

    </lg>

    <lg type="cobla" n="2" xml:id="C065-lg2">

     <l n="9" xml:id="C065-l9"> Si com lom cech no conex njt nj jorn </l>

     <l n="10" xml:id="C065-l10"> Si dels vehents ell no es auisat </l>

     <l n="11" xml:id="C065-l11"> Dira y amor no se qual ma sobrat </l>

     <l n="12" xml:id="C065-l12"> Haian auis dels quim <choice>
       <abbr>stā</abbr>
       <expan>stan</expan>
      </choice> antorn </l>

     <pb n="63v"
      facs="https://www.cervantesvirtual.com/obra-visor/les-obres-de-mossen-ausias-march-sont-acabada-de-scriure-la-present-obra-per-m-pere-vilaraso-prevere-per-manament-de-lilltre-sor-almirant-de-napols-manuscrit--0/html/00488f44-82b2-11df-acc7-002185ce6064_133.html"/>

     <l n="13" xml:id="C065-l13"> Yo desitx molt quamor <choice>
       <abbr>mabādonas</abbr>
       <expan>mabandonas</expan>
      </choice>
     </l>

     <l n="14" xml:id="C065-l14"> Car sola es la causa de mon mal </l>

     <l n="15" xml:id="C065-l15"> Mas <choice>
       <abbr>đ</abbr>
       <expan>de</expan>
      </choice> poder jra no te cabal </l>

     <l n="16" xml:id="C065-l16"> Quan contramor yo tant <choice>
       <abbr>ḿ</abbr>
       <expan>me</expan>
      </choice> rebellas </l>

    </lg>

    <lg type="cobla" n="3" xml:id="C065-lg3">

     <l n="17" xml:id="C065-l17"> E si remey ama dolor trobas </l>

     <l n="18" xml:id="C065-l18"> Fora content car jon desitx exir </l>

     <l n="19" xml:id="C065-l19"> Los vostres fets me fan vos auorrir </l>

     <l n="20" xml:id="C065-l20"> E nos pot fer <choice>
       <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice> ab vos praticas </l>

     <l n="21" xml:id="C065-l21"> Mon partiment no puch acabar </l>

     <l n="22" xml:id="C065-l22"> En vos pensant <choice>
      <abbr>strāy</abbr>
       <expan>strany</expan>
      </choice>
      <choice>
       <abbr>turmēt</abbr>
       <expan>turment</expan>
      </choice> ne tench </l>

     <l n="23" xml:id="C065-l23"> E <choice>
       <abbr>quāt</abbr>
       <expan>quant</expan>
      </choice> lestat de mj saber mestrench </l>

     <l n="24" xml:id="C065-l24"> Ladonchs yo caych en mj pus jgnorar </l>

    </lg>

    <lg type="cobla" n="4" xml:id="C065-lg4">

     <l n="25" xml:id="C065-l25"> No se remey potent mj consolar </l>

     <l n="26" xml:id="C065-l26"> Si altramor noua no conseguesch </l>

     <l n="27" xml:id="C065-l27"> O tu amor colp vell gorex abfresch </l>

     <l n="28" xml:id="C065-l28"> O de <choice>
       <abbr>aꝗst</abbr>
       <expan>aquest</expan>
      </choice> me vulles <choice>
       <abbr>bādonar</abbr>
       <expan>bandonar</expan>
      </choice>
     </l>

     <l n="29" xml:id="C065-l29"> Veies <choice>
       <abbr>mō</abbr>
       <expan>mon</expan>
      </choice> cor en quina dolor jau </l>

     <l n="30" xml:id="C065-l30"> En ton hostal no merex tan dur lit </l>

     <l n="31" xml:id="C065-l31"> Qui es lom viu don sies mils seruit </l>

     <l n="32" xml:id="C065-l32"> Tant es humil quesforç nol pot fer brau </l>

    </lg>

    <lg type="cobla" n="5" xml:id="C065-lg5">

     <l n="33" xml:id="C065-l33"> Plagues <choice>
       <abbr>ađu</abbr>
       <expan>adeu</expan>
      </choice>
      <choice>
       <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice> ab mj matex fes pau </l>

     <l n="34" xml:id="C065-l34"> Mi <choice>
       <abbr>acordāt</abbr>
       <expan>acordant</expan>
      </choice> ab cor ferm ala mort </l>

     <l n="35" xml:id="C065-l35"> Yo mjr epens si al <choice>
       <abbr>mō</abbr>
       <expan>mon</expan>
      </choice> ha cas <choice>
       <abbr>tā</abbr>
       <expan>tan</expan>
      </choice> fort </l>

     <l n="36" xml:id="C065-l36"> Com <choice>
       <abbr>đ</abbr>
       <expan>de</expan>
      </choice> amor e dira fer sclau </l>

     <l n="37" xml:id="C065-l37"> Afany te sats lom dos senyos siruent </l>

     <pb n="64r"
      facs="https://www.cervantesvirtual.com/obra-visor/les-obres-de-mossen-ausias-march-sont-acabada-de-scriure-la-present-obra-per-m-pere-vilaraso-prevere-per-manament-de-lilltre-sor-almirant-de-napols-manuscrit--0/html/00488f44-82b2-11df-acc7-002185ce6064_134.html"/>

     <l n="38" xml:id="C065-l38"> E quant mes donchs essent ells enemichs </l>

     <l n="39" xml:id="C065-l39"> Quant hu <choice>
       <abbr>cōplanch</abbr>
       <expan>complanch</expan>
      </choice> laltres ves mj jnichs </l>

     <l n="40" xml:id="C065-l40"> No se com bast complaures egualment </l>

    </lg>

    <lg type="tornada" n="6" xml:id="C065-lg6">

     <l n="41" xml:id="C065-l41"> O ffollamor si es ver quel jouent </l>

     <l n="42" xml:id="C065-l42"> Es ocasio de tu amj forçar </l>

     <l n="43" xml:id="C065-l43"> Si be lo mon res tant no pot prear </l>

     <l n="44" xml:id="C065-l44"> Velleam plau qui <choice>
       <abbr>đ</abbr>
       <expan>de</expan>
      </choice> mals es sement </l>

    </lg>

   </div>
  </body>
 </text>
