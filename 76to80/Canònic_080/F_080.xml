   <text>
      <body>
         <div type="poem" xml:id="C080-F080">
            
            <pb n="84v"
               facs="https://www.cervantesvirtual.com/obra-visor/poesias-de-ausias-y-mena-manuscrito--0/html/0165fe84-82b2-11df-acc7-002185ce6064_172.html"/>

            <lg type="cobla" n="1" xml:id="C080-lg1">

               <l n="1" xml:id="C080-l1">
                  <c rend="large">T</c>Ot laurador es pagat del jornal </l>

               <l n="2" xml:id="C080-l2"> E laduocat quj pert lo guanyat plet </l>

               <l n="3" xml:id="C080-l3"> Yo per serujr amor romanch desfet </l>

               <l n="4" xml:id="C080-l4"> De tot quant he que serujr nomencal </l>

               <l n="5" xml:id="C080-l5"> He fet senyor del seny amon uoler </l>

               <l n="6" xml:id="C080-l6"> Vehent amor de mon seny mal serujt </l>

               <l n="7" xml:id="C080-l7"> Rapac lefet e deu apart iaqujt </l>

               <l n="8" xml:id="C080-l8"> E son setzanys que lo guardo esper </l>

            </lg>

            <pb n="85r"
               facs="https://www.cervantesvirtual.com/obra-visor/poesias-de-ausias-y-mena-manuscrito--0/html/0165fe84-82b2-11df-acc7-002185ce6064_173.html"/>

            <lg type="tornada" n="2" xml:id="C080-lg2">

               <l n="9" xml:id="C080-l9">
                  <c rend="large">A</c>mor amor poch es uostre poder </l>

               <l n="10" xml:id="C080-l10"> Per altre hom com yo fer tant amar </l>

               <l n="11" xml:id="C080-l11"> Anau anau uostres armes prouar </l>

               <l n="12" xml:id="C080-l12"> Encontraquell quj uostre no uol ser </l>

            </lg>

         </div>
      </body>
   </text>
