 <text>
  <body>
   <div type="poem" xml:id="C035-K035">

    <pb n="34v" facs="http://www.cervantesvirtual.com/obra-visor/les-obres-de-mossen-ausias-march-sont-acabada-de-scriure-la-present-obra-per-m-pere-vilaraso-prevere-per-manament-de-lilltre-sor-almirant-de-napols-manuscrit--0/html/00488f44-82b2-11df-acc7-002185ce6064_75.html"/>

    <lg type="cobla" n="1" xml:id="C035-lg1">
     <l n="1" xml:id="C035-l1"> s Ja cascu per ben hoir atent </l>
     <l n="2" xml:id="C035-l2"> E no ymagin <choice>
      <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice> jol vulla decebre </l>
     <l n="3" xml:id="C035-l3"> Car de tot cert per mj pora pecebre </l>
     <l n="4" xml:id="C035-l4"> Grans nouitats e denou sentiment </l>
     <l n="5" xml:id="C035-l5"> Tals <choice>
      <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice> no se los <choice>
       <abbr>minyôs</abbr>
       <expan>minyons</expan>
      </choice> quils atrassen </l>
     <l n="6" xml:id="C035-l6"> Mas solament restan mj lo sentir </l>
     <l n="7" xml:id="C035-l7"> Si col malalt qui sa dolor sab dir </l>
     <l n="8" xml:id="C035-l8"> No sab los mals qui la mort mj <choice>
      <abbr>ꝑcassan</abbr>
       <expan>percassan</expan>
      </choice></l>
    </lg>

    <lg type="cobla" n="2" xml:id="C035-lg2">
     <l n="9" xml:id="C035-l9"><choice>
       <abbr><c rend="large">A</c>ꝗlls</abbr>
       <expan>Aquells</expan>
      </choice> affanys quels amados acassen </l>
     <l n="10" xml:id="C035-l10"> E son <choice>
       <abbr>comûs</abbr>
       <expan>comuns</expan>
      </choice> e quasi manjfests </l>
     <l n="11" xml:id="C035-l11"> No son en mj ne de <choice>
       <abbr>semblâts</abbr>
       <expan>semblants</expan>
      </choice><choice>
       <abbr>daꝗsts</abbr>
       <expan>daquests</expan>
      </choice></l>
     <l n="12" xml:id="C035-l12"> Nom fan paor ne sols mj no menassan </l>
     <l n="13" xml:id="C035-l13"> Dolos etals quen amados no venen </l>
     <l n="14" xml:id="C035-l14"> Car yo peresch com ja no pusch amar </l>
     <l n="15" xml:id="C035-l15"> Solament bast mj demort ayrar </l>
     <l n="16" xml:id="C035-l16"> Per molts defalts <choice>
       <abbr>quê</abbr>
       <expan>quen</expan>
      </choice> ira mj <choice>
       <abbr>encenê</abbr>
       <expan>encenen</expan></choice>
     </l>
    </lg>

    <lg type="cobla" n="3" xml:id="C035-lg3">
     <l n="17" xml:id="C035-l17"> <c rend="large">N</c>o se quins son los delits <choice>
       <abbr>quî</abbr>
       <expan>quim</expan>
      </choice><choice>
       <abbr>sostenê</abbr>
       <expan>sostenen</expan>
      </choice></l>
     <l n="18" xml:id="C035-l18"> Com <choice>
       <abbr>nô</abbr>
       <expan>nom</expan>
      </choice> acuyt metre lo <choice>
       <abbr>mô</abbr>
       <expan>mon</expan>
      </choice> apart </l>
     <l n="19" xml:id="C035-l19"> Car de yrarme ya mes nom vetx fart </l>
     <l n="20" xml:id="C035-l20"> Pensau los bens <choice>
      <abbr>ꝗnesta</abbr>
       <expan>quenesta</expan>
      </choice> pensas <choice>
       <abbr>prenê</abbr>
       <expan>prenen</expan>
      </choice> </l>
     <l n="21" xml:id="C035-l21"> Morir me plau car nom te prou lo viure </l>
     <l n="22" xml:id="C035-l22"> Lo mils demj es com en res no pens </l>
     
     <pb n="35r" facs="http://www.cervantesvirtual.com/obra-visor/les-obres-de-mossen-ausias-march-sont-acabada-de-scriure-la-present-obra-per-m-pere-vilaraso-prevere-per-manament-de-lilltre-sor-almirant-de-napols-manuscrit--0/html/00488f44-82b2-11df-acc7-002185ce6064_76.html"/>
    
     <l n="23" xml:id="C035-l23"> E tant com pusch de pensar mj <choice>
      <abbr>ƌfens</abbr>
       <expan>defens</expan>
      </choice> </l>
     <l n="24" xml:id="C035-l24"> Conçell <choice>
       <abbr>nô</abbr>
       <expan>nom</expan>
      </choice> cal sim dech <choice>
       <abbr>ꝑ</abbr>
       <expan>per</expan>
      </choice> foll scriure </l>
    </lg>

    <lg type="cobla" n="4" xml:id="C035-lg4">
     <l n="25" xml:id="C035-l25"> <c rend="large">S</c>i col prens qui nos de mort deslliure </l>
     <l n="26" xml:id="C035-l26"> Vehent se prop <choice>
      <abbr>daꝗll</abbr>
       <expan>daquell</expan>
      </choice> seu jorn darrer </l>
     <l n="27" xml:id="C035-l27"> No prega deu li sia merçener </l>
     <l n="28" xml:id="C035-l28"> E sap <choice>
      <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice> va hon null hom se pot riure </l>
     <l n="29" xml:id="C035-l29"> Ne pren ame <choice>
      <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice> futx a la merce </l>
     <l n="30" xml:id="C035-l30"> Damor anqui sanclou tot lo meu esser </l>
     <l n="31" xml:id="C035-l31"> Tant enues ell me so volgut jrexer </l>
     <l n="32" xml:id="C035-l32"> Per lo fallir qui es comes per me </l>
    </lg>

    <lg type="cobla" n="5" xml:id="C035-lg5">
     <l n="33" xml:id="C035-l33"> <c rend="large">S</c>i com lo pech qui ha penssat dins se </l>
     <l n="34" xml:id="C035-l34"> Del hom sabent a son saber atenyer </l>
     <l n="35" xml:id="C035-l35"> E <choice>
       <abbr>cô</abbr>
       <expan>com</expan>
      </choice> veu si <choice>
       <abbr>quêtant</abbr>
       <expan>quentant</expan>
      </choice> nos pot <choice>
       <abbr>empêyer</abbr>
       <expan>empenyer</expan>
      </choice> </l>
     <l n="36" xml:id="C035-l36"> Lo malahex e si matex <choice>
       <abbr>dô</abbr>
       <expan>don</expan>
      </choice> ve </l>
     <l n="37" xml:id="C035-l37"> Ne <choice>
       <abbr>prê</abbr>
       <expan>pren</expan>
      </choice> amj no bastant ser ates </l>
     <l n="38" xml:id="C035-l38"> Lay hon tots <choice>
       <abbr>têps</abbr>
       <expan>temps</expan>
      </choice> he tingut <choice>
       <abbr>mô</abbr>
       <expan>mon</expan>
      </choice> camj </l>
     <l n="39" xml:id="C035-l39"> Tot per defalt ques atrobat enmj </l>
     <l n="40" xml:id="C035-l40"> Ayr amor e volgra ser no res </l>
    </lg>

    <lg type="tornada" n="6" xml:id="C035-lg6">
     <l n="41" xml:id="C035-l41"> <c rend="large">L</c>ir entre carts lo be quim es defes </l>
     <l n="42" xml:id="C035-l42"> Amor lom feu amiprimer<note type="editorial">amprimer (sic)</note> desijar </l>
     <l n="43" xml:id="C035-l43"> E <choice>
       <abbr>follamêt</abbr>
       <expan>follament</expan>
      </choice>
      <choice>
       <abbr>ƌ</abbr>
       <expan>de</expan>
      </choice> tant preu stimar </l>
     <l n="44" xml:id="C035-l44"> Que no ymagin quauer se pogues </l>
    </lg>

   </div>
  </body>
 </text>
