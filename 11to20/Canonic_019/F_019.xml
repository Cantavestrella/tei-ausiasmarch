   <text>
      <body>
         <pb n="19r"
            facs="http://www.cervantesvirtual.com/obra-visor/poesias-de-ausias-y-mena-manuscrito--0/html/0165fe84-82b2-11df-acc7-002185ce6064_41.html"/>
         <div type="poem"  xml:id="C019-F019">
            <lg type="cobla" n="1" xml:id="C019-01">
               
               <l n="1" xml:id="C019-1"> Hohiu hoiu tots los qui be amats
               </l>
               
               <l n="2" xml:id="C019-2"> E planyeu mj si deig esser plangut
               </l>
               
               <l n="3" xml:id="C019-3"> E puys veheu si es tal cas uengut
               </l>
               
               <l n="4" xml:id="C019-4"> En los presents nen los qui son passats
               </l>
               
               <l n="5" xml:id="C019-5"> Doleu uos donchs de mj uostre semblant
               </l>
               
               <l n="6" xml:id="C019-6"> Ensoferir la dolor delitable
               </l>
               
               <l n="7" xml:id="C019-7"> Car tost de mj se dolrra lo diable
               </l>
               
               <l n="8" xml:id="C019-8"> Com ueura mj semblant mal del passant
               </l>
            </lg>
            <pb n="19v"
               facs="http://www.cervantesvirtual.com/obra-visor/poesias-de-ausias-y-mena-manuscrito--0/html/0165fe84-82b2-11df-acc7-002185ce6064_42.html"/>
            <lg type="cobla" n="2" xml:id="C019-02">
               
               <l n="9" xml:id="C019-9"> Qujes lom viu tal dolor suffertant<del rend="overstrike">s</del>
               </l>
               
               <l n="10" xml:id="C019-10"> Que desig co de que se desespera
               </l>
               
               <l n="11" xml:id="C019-11"> Aytant es greu que no par cosa uera
               </l>
               
               <l n="12" xml:id="C019-12"> Desijar co de ques desesperant
               </l>
               
               <l n="13" xml:id="C019-13"> Ne son ganat de mon mal estament
               </l>
               
               <l n="14" xml:id="C019-14"> Tot quant pratich tornar me sent en dan
               </l>
               
               <l n="15" xml:id="C019-15"> Menys de poder me trob hauent lo gran
               </l>
               
               <l n="16" xml:id="C019-16"> Car nom esforc<del rend="overstrike">h</del> per mostrar mon talent
               </l>
            </lg>
            <lg type="cobla" n="3" xml:id="C019-03">
               
               <l n="17" xml:id="C019-17"> Mon primer mal es mon esperdjment
               </l>
               
               <l n="18" xml:id="C019-18"> Per quem ahir e per no res macus
               </l>
               
               <l n="19" xml:id="C019-19"> E lo segon es terrible reffus
               </l>
               
               <l n="20" xml:id="C019-20"> Que uos mostrau sius feyanqueriment
               </l>
               
               <l n="21" xml:id="C019-21"> Portat me trob amolt prop de ma fi
               </l>
               
               <l n="22" xml:id="C019-22"> Puys mon uoler cas jnpossible guarda
               </l>
               
               <l n="23" xml:id="C019-23"> No tardara lora que ia fos tarda
               </l>
               
               <l n="24" xml:id="C019-24"> Que tendrels peus enlauorrit camj
               </l>
            </lg>
            <lg type="cobla" n="4" xml:id="C019-04">
               
               <l n="25" xml:id="C019-25"> Si per null temps encontramor falli
               </l>
               
               <l n="26" xml:id="C019-26"> Yon son repres planyent men lenemich
               </l>
               
               <l n="27" xml:id="C019-27"> E may uers mj poguj esser amjch
               </l>
               
               <l n="28" xml:id="C019-28"> Car per null temps poder hi despenguj
               </l>
               
               <l n="29" xml:id="C019-29"> Hoc lo uoler me trob en habundanca
               </l>
               
               <l n="30" xml:id="C019-30"> Mas del poder non se <choice><abbr>pꝯ</abbr><expan>pus</expan></choice> enpobrit
               </l>
               
               <l n="31" xml:id="C019-31"> Car yo peresch e son tan defallit
               </l>
               
               <l n="32" xml:id="C019-32"> Que no puch dir en uos es masperanca
               </l>
            </lg>
            <pb n="20r"
               facs="http://www.cervantesvirtual.com/obra-visor/poesias-de-ausias-y-mena-manuscrito--0/html/0165fe84-82b2-11df-acc7-002185ce6064_43.html"/>
            <lg type="cobla" n="5" xml:id="C019-05">
               
               <l n="33" xml:id="C019-33"> Una sabor dagre dolc amor lanca
               </l>
               
               <l n="34" xml:id="C019-34"> Que lo meu gust departir les no sap
               </l>
               
               <l n="35" xml:id="C019-35"> Dins mos delits dolor mortal hy cap
               </l>
               
               <l n="36" xml:id="C019-36"> E tal dolor ab delit ha liganca
               </l>
               
               <l n="37" xml:id="C019-37"> Mas yom reprench com parlar maplagut
               </l>
               
               <l n="38" xml:id="C019-38"> De co quen mj no basta la sciencia
               </l>
               
               <l n="39" xml:id="C019-39"> Sobres amor me porta ignoscencia
               </l>
               
               <l n="40" xml:id="C019-40"> Vull /e desuull sens cas esdeuengut
               </l>
            </lg>
            <lg type="cobla" n="6" xml:id="C019-06">
               
               <l n="41" xml:id="C019-41"> Lir entre carts creeu lamador mut
               </l>
               
               <l n="42" xml:id="C019-42"> Y al <choice><abbr>cambiȃt</abbr><expan>cambiant</expan></choice> de punt en punt color
               </l>
               
               <l n="43" xml:id="C019-43"> E al pauruch com se menbra damor
               </l>
               
               <l n="44" xml:id="C019-44"> Del atreuit sia son temps perdut
               </l>
            </lg>
         </lg>
