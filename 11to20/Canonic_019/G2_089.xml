   <text>
      <body>
         <pb n="129r"
            facs="http://www.cervantesvirtual.com/obra-visor/mossen-ausias-march-poeta-manuscrito--0/html/01f733fe-82b2-11df-acc7-002185ce6064_268.html"/>
         <div type="poem" xml:id="C019-G2089">
            <lg type="cobla" n="1" xml:id="C019-01">
               
               <l n="1" xml:id="C019-1"> Hoiu hoiu / tots los qui be amats
               </l>
               
               <l n="2" xml:id="C019-2"> eplanyeu mi / si dech <choice><abbr>esş</abbr><expan>esser</expan></choice> planygut
               </l>
               
               <l n="3" xml:id="C019-3"> epuis veeu / si es tal cas vengut
               </l>
               
               <l n="4" xml:id="C019-4"> enlos <choice><abbr>ṗsȇts</abbr><expan>presents</expan></choice> / nen los qui sonpassats
               </l>
               
               <l n="5" xml:id="C019-5"> Doleu vos donchs / demi <choice><abbr>vrȇ</abbr><expan>vostre</expan></choice> semblant
               </l>
               
               <l n="6" xml:id="C019-6"> ensoferir ladolor /delictable
               </l>
               
               <l n="7" xml:id="C019-7"> car tost demi / sedolrralo diable
               </l>
               
               <l n="8" xml:id="C019-8"> com veure ami / senblant mal dell passant
               </l>
            </lg>
            <lg type="cobla" n="2" xml:id="C019-02">
               
               <l n="9" xml:id="C019-9"> Qui eslomviu / tal dolor sufertant
               </l>
               
               <l n="10" xml:id="C019-10"> que desig so / deque no agasper
               </l>
               
               <l n="11" xml:id="C019-11"> aytant es greu quepar nosia ver
               </l>
               
               <l n="12" xml:id="C019-12"> desijar so / deques desesperant
               </l>
               
               <l n="13" xml:id="C019-13"> He songanat / demonmla <choice><abbr>stamȇt</abbr><expan>stament</expan></choice>
               </l>
               
               <l n="14" xml:id="C019-14"> toquant pratich / tornar mesent en dan
               </l>
               
               <l n="15" xml:id="C019-15"> menys depoder / metrob auent logran
               </l>
               
               <l n="16" xml:id="C019-16"> car nom esforç / per mostrar mon talent
               </l>
            </lg>
            <lg type="cobla" n="3" xml:id="C019-03">
               
               <l n="17" xml:id="C019-17"> Monprimer mal / es mon sperdiment
               </l>
               
               <l n="18" xml:id="C019-18"> perquem ahir / eper norres macus
               </l>
               
               <l n="19" xml:id="C019-19"> elosegon / esterrible refus
               </l>
               
               <l n="20" xml:id="C019-20"> quevos mostrau / <choice><abbr>siꝯ</abbr><expan>sius</expan></choice> feyan queriment
               </l>
               
               <l n="21" xml:id="C019-21"> <choice><abbr>Portȃt</abbr><expan>Portant</expan></choice> metrop / amolt prop de mafi
               </l>
               
               <l n="22" xml:id="C019-22"> puis mon voler / casinpossible guarda
               </l>
               
               <l n="23" xml:id="C019-23"> notadaralora / que ja fos tarda
               </l>
               
               <l n="24" xml:id="C019-24"> que tendrels peus / enlavorrit cami
               </l>
            </lg>
            <pb n="129v"
               facs="http://www.cervantesvirtual.com/obra-visor/mossen-ausias-march-poeta-manuscrito--0/html/01f733fe-82b2-11df-acc7-002185ce6064_269.html"/>
            <lg type="cobla" n="4" xml:id="C019-04">
               
               <l n="25" xml:id="C019-25">  Si per null temps / encontramor falli
               </l>
               
               <l n="26" xml:id="C019-26"> bem arepres / planyent men lenemich
               </l>
               
               <l n="27" xml:id="C019-27"> emay vers mi / pogui <choice><abbr>eß</abbr><expan>esser</expan></choice> amich
               </l>
               
               <l n="28" xml:id="C019-28"> car per null temps / poder hi despengui
               </l>
               
               <l n="29" xml:id="C019-29"> hoc lo voler / metrop en abundança
               </l>
               
               <l n="30" xml:id="C019-30"> mas delpoder / nonse pus enpobrit
               </l>
               
               <l n="31" xml:id="C019-31"> car yo peresch / eso tantdefallit
               </l>
               
               <l n="32" xml:id="C019-32"> que no puchdir / envos es masperança
               </l>
            </lg>
            <lg type="cobla" n="5" xml:id="C019-05">
               
               <l n="33" xml:id="C019-33"> Una sabor / dagredolç amor llansa
               </l>
               
               <l n="34" xml:id="C019-34"> quelo meu gust departir les nosap
               </l>
               
               <l n="35" xml:id="C019-35"> dins mos delits dolor mortal ycap
               </l>
               
               <l n="36" xml:id="C019-36"> etal dolor ab delit alligança
               </l>
               
               <l n="37" xml:id="C019-37"> Mas yom reprench / com parlar maplagut
               </l>
               
               <l n="38" xml:id="C019-38"> deço quen mi nobastalasciencia
               </l>
               
               <l n="39" xml:id="C019-39"> sobres amor / meporta ignocencia
               </l>
               
               <l n="40" xml:id="C019-40"> vull e des vull / sens cas sdeuengut
               </l>
               
            </lg>
         </lg>
