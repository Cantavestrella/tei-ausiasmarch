   <text>
      <body>
         <pb n="24"
            facs="http://www.cervantesvirtual.com/portales/ausias_march/obra-visor/canconer-de-obres-de-mossen-ausias-march-manuscrito--0/html/017bdf6a-82b2-11df-acc7-002185ce6064_26.html"/>
         <lg type="poem" xml:id="C012-N012">
            <lg type="cobla" n="1" xml:id="C012-01">
               
               <l n="1" xml:id="C012-1"> &#182;  Ja no esper que sia amat
               </l>
               
               <l n="2" xml:id="C012-2"> Car mon voler <choice><abbr>noꝯ</abbr><expan>nous</expan></choice> ven report
               </l>
               
               <l n="3" xml:id="C012-3"> Per mon esforc quj nos tan fort
               </l>
               
               <l n="4" xml:id="C012-4"> Que dir <choice><abbr>queꝯ</abbr><expan>queus</expan></choice> am aya gosat
               </l>
               
               <l n="5" xml:id="C012-5"> Per vos me so mes en amar
               </l>
               
               <l n="6" xml:id="C012-6"> E mon hull nom voll descobrir
               </l>
               
               <l n="7" xml:id="C012-7"> Moltys menys ma lengua volradir
               </l>
               
               <l n="8" xml:id="C012-8"> Co quel gest no gossa mostrar
               </l>
            </lg>
            <lg type="cobla" n="2" xml:id="C012-02">
               
               <l n="9" xml:id="C012-9"> Volgerauer lull tan esforcat
               </l>
               
               <l n="10" xml:id="C012-10"> Ab <choice><abbr>queꝯ</abbr><expan>queus</expan></choice> mostras lo que dins port
               </l>
               
               <l n="11" xml:id="C012-11"> E <choice><abbr>queꝯ</abbr><expan>queus</expan></choice> fes venir enacort
               </l>
               
               <l n="12" xml:id="C012-12"> Que diu lesguart enamorat
               </l>
               
               <l n="13" xml:id="C012-13"> Demon cor no puch compte far
               </l>
               
               <l n="14" xml:id="C012-14"> Ne dema lengua per lo dir
               </l>
               
               <l n="15" xml:id="C012-15"> Dient ques deu lull atreuir
               </l>
               
               <l n="16" xml:id="C012-16"> Car mils sen pot atras tornar
               </l>
            </lg>
            <lg type="cobla" n="3" xml:id="C012-03">
               
               <l n="17" xml:id="C012-17"> Sobre vos es tot be cargat
               </l>
               
               <l n="18" xml:id="C012-18"> Mas feu semblant no voler part
               </l>
               
               <l n="19" xml:id="C012-19"> Dela merce quil carrech tort
               </l>
               
               <l n="20" xml:id="C012-20"> Ab son bell gin ha redrecat
               </l>
               
               <l n="21" xml:id="C012-21"> Quj pot ni deu tal fex portar
               </l>
               
               <l n="22" xml:id="C012-22"> Volent tal pena sofferir
               </l>
               
               <l n="23" xml:id="C012-23"> E la merce noy vol suplir
               </l>
               
               <l n="24" xml:id="C012-24"> Per lo gran carrech parencar
               </l>
            </lg>
            <pb n="25"
               facs="http://www.cervantesvirtual.com/portales/ausias_march/obra-visor/canconer-de-obres-de-mossen-ausias-march-manuscrito--0/html/017bdf6a-82b2-11df-acc7-002185ce6064_27.html"/>
            <lg type="cobla" n="4" xml:id="C012-04">
               
               <l n="25" xml:id="C012-25"> Quj es aquell tan mal fadat
               </l>
               
               <l n="26" xml:id="C012-26"> Que sens esper li fall conort
               </l>
               
               <l n="27" xml:id="C012-27"> E no sesfort contral cas fort
               </l>
               
               <l n="28" xml:id="C012-28"> Puys en aquell es desperat
               </l>
               
               <l n="29" xml:id="C012-29"> Eyo nom puch aconortar
               </l>
               
               <l n="30" xml:id="C012-30"> Ans prench tot giny perno sentir
               </l>
               
               <l n="31" xml:id="C012-31"> Los grans affanys quim sent venir
               </l>
               
               <l n="32" xml:id="C012-32"> En part venguts per no gosar
               </l>
            </lg>
            <lg type="cobla" n="5" xml:id="C012-05">
               
               <l n="33" xml:id="C012-33"> No puch recollir lo desgrat
               </l>
               
               <l n="34" xml:id="C012-34"> Quem ve demj sens ferme tort
               </l>
               
               <l n="35" xml:id="C012-35"> Ffaent <choice><abbr>mȇys</abbr><expan>menys</expan></choice> mal veg passar mort
               </l>
               
               <l n="36" xml:id="C012-36"> Ha molt mesquj desuenturat
               </l>
               
               <l n="37" xml:id="C012-37"> Car nom so mes en asajar
               </l>
               
               <l n="38" xml:id="C012-38"> Esser lo mils nat en hahuyr
               </l>
               
               <l n="39" xml:id="C012-39"> Perno poder mj en ardir
               </l>
               
               <l n="40" xml:id="C012-40"> En passions damor mostrar
               </l>
            </lg>
            <lg type="cobla" n="6" xml:id="C012-06">
               
               <l n="41" xml:id="C012-41"> Les dones prech vullen pensar
               </l>
               
               <l n="42" xml:id="C012-42"> Cascunab qujn esguart lamir
               </l>
               
               <l n="43" xml:id="C012-43"> En lo gest me vull en ardir
               </l>
               
               <l n="44" xml:id="C012-44"> Pusque amor ma tolt parlar
               </l>
            </lg>
         </lg>
