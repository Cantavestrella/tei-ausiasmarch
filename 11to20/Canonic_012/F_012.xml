   <text>
      <body>
         <pb n="11v"
            facs="http://www.cervantesvirtual.com/obra-visor/poesias-de-ausias-y-mena-manuscrito--0/html/0165fe84-82b2-11df-acc7-002185ce6064_26.html"/>
         <div type="poem" xml:id="C012-F012">
            <lg type="cobla" n="1" xml:id="C012-01">
               
               <l n="1" xml:id="C012-1"> Ja no esper que siamat <add place="margin left">No es de ausias march.</add><note type="editorial">Amb una lletra diferent.</note>
               </l>
                              
               <l n="2" xml:id="C012-2"> Car mon uoler nous ueu report
               </l>
               
               <l n="3" xml:id="C012-3"> Per mon esforc quj nos tan fort
               </l>
               
               <l n="4" xml:id="C012-4"> Que dir queus am haia gosat
               </l>
               
               <l n="5" xml:id="C012-5"> Per uos me so mes en amar
               </l>
               
               <l n="6" xml:id="C012-6"> E mon hull nom uol descobrir
               </l>
               
               <l n="7" xml:id="C012-7"> Molt menys ma lengua volrra dir
               </l>
               
               <l n="8" xml:id="C012-8"> Co quel gest no guosa mostrar
               </l>
            </lg>
            <lg type="cobla" n="2" xml:id="C012-02">
               
               <l n="9" xml:id="C012-9"> Volgrauer <del rend="overstrike">ll</del> lull tan esforcat
               </l>
               
               <l n="10" xml:id="C012-10"> Ab queus mostras lo que dins port
               </l>
               
               <l n="11" xml:id="C012-11"> E <choice><abbr>queꝯ</abbr><expan>queus</expan></choice> fes uenir en acort
               </l>
               
               <l n="12" xml:id="C012-12"> Que diu lesguart enamorat
               </l>
               
               <l n="13" xml:id="C012-13"> De mon cor no puch compte far
               </l>
               
               <l n="14" xml:id="C012-14"> Ne de malengua per lo dir
               </l>
               
               <l n="15" xml:id="C012-15"> Dient ques deu lull atreujr
               </l>
               
               <l n="16" xml:id="C012-16"> Car mjlls sen pot atras tornar
               </l>
            </lg>
            <lg type="cobla" n="3" xml:id="C012-03">
               
               <l n="17" xml:id="C012-17"> Sobre uos <del rend="overstrike">l</del> es tot be cargat
               </l>
               
               <l n="18" xml:id="C012-18"> Mas feu senblant no uoler port
               </l>
               
               <l n="19" xml:id="C012-19"> Dela merce quil carrech tort
               </l>
               
               <l n="20" xml:id="C012-20"> Ab son bell g<del rend="overstrike">u</del>jny ha redrecat
               </l>
               
               <l n="21" xml:id="C012-21"> Qui pot ne deu tal feix portar
               </l>
               
               <l n="22" xml:id="C012-22"> Volent tal pena sofrir
               </l>
               
               <l n="23" xml:id="C012-23"> E la merce noy uol suplir
               </l>
               
               <l n="24" xml:id="C012-24"> Per lo gran carrech parencar
               </l>
            </lg>
            <pb n="12r"
               facs="http://www.cervantesvirtual.com/obra-visor/poesias-de-ausias-y-mena-manuscrito--0/html/0165fe84-82b2-11df-acc7-002185ce6064_27.html"/>
            <lg type="cobla" n="4" xml:id="C012-04">
               
               <l n="25" xml:id="C012-25"> Qui es aquell tan malfadat
               </l>
               
               <l n="26" xml:id="C012-26"> Que sens esper li fall conort
               </l>
               
               <l n="27" xml:id="C012-27"> E no sesforc contral cas fort
               </l>
               
               <l n="28" xml:id="C012-28"> Puys en aquell es desperat
               </l>
               
               <l n="29" xml:id="C012-29"> E yo nom puch aconortar
               </l>
               
               <l n="30" xml:id="C012-30"> Ans prench tot giny <choice><abbr>ꝑ</abbr><expan>per</expan></choice> no sentir
               </l>
               
               <l n="31" xml:id="C012-31"> Los grans afanys quim sent uenir
               </l>
               
               <l n="32" xml:id="C012-32"> En part uenguts per no gosar
               </l>
            </lg>
            <lg type="cobla" n="5" xml:id="C012-05">
               
               <l n="33" xml:id="C012-33"> No puch recollir lo desgrat
               </l>
               
               <l n="34" xml:id="C012-34"> Quem ue de mj sens ferme tort
               </l>
               
               <l n="35" xml:id="C012-35"> Ffaent menys mal vey passar mort
               </l>
               
               <l n="36" xml:id="C012-36"> A molt mesquj desuenturat
               </l>
               
               <l n="37" xml:id="C012-37"> Car nom so mes en assaiar
               </l>
               
               <l n="38" xml:id="C012-38"> Desser lo mjls nat en ahuyr
               </l>
               
               <l n="39" xml:id="C012-39"> Per no poder mj enardir
               </l>
               
               <l n="40" xml:id="C012-40"> En passions damor mostrar
               </l>
            </lg>
            <lg type="cobla" n="6" xml:id="C012-06">
               
               <l n="41" xml:id="C012-41"> Les dones prech uullen pensar
               </l>
               
               <l n="42" xml:id="C012-42"> Cascunab quin esguart la mjr
               </l>
               
               <l n="43" xml:id="C012-43"> En lo gest me vull enardir
               </l>
               
               <l n="44" xml:id="C012-44"> Puys que amor ma tolt parlar
               </l>
            </lg>
         </lg>
