   <text>
      <body>
         <pb n="12r"
            facs="http://www.cervantesvirtual.com/obra-visor/obres-dausias-march-manuscrito--0/html/01f79830-82b2-11df-acc7-002185ce6064_27.html"/>
         <div type="poem" xml:id="C012-B013">
            <lg type="cobla" n="1" xml:id="C012-01">

               <l n="1" xml:id="C012-1"> iAno sper que sia amat </l>

               <l n="2" xml:id="C012-2"> Car mon voler nous ven report </l>

               <l n="3" xml:id="C012-3"> Per mon sforç qui nos tan fort </l>

               <l n="4" xml:id="C012-4"> Que dir queus am aia gosat </l>
               <pb n="12v"
                  facs="http://www.cervantesvirtual.com/obra-visor/obres-dausias-march-manuscrito--0/html/01f79830-82b2-11df-acc7-002185ce6064_28.html"/>
               <l n="5" xml:id="C012-5"> Per vos me so mes en amar </l>

               <l n="6" xml:id="C012-6"> E mon vll nom vol descobrir </l>

               <l n="7" xml:id="C012-7"> Molt <choice>
                     <abbr>mȇys</abbr>
                     <expan>menys</expan>
                  </choice> ma lengua volrra dir </l>

               <l n="8" xml:id="C012-8"> Ço quel gest no gosa mostrar </l>
            </lg>
            <lg type="cobla" n="2" xml:id="C012-02">

               <l n="9" xml:id="C012-9"> Volgra lull tan sforçat </l>

               <l n="10" xml:id="C012-10"> Ab queus mostras lo <choice>
                     <abbr>ꝗ</abbr>
                     <expan>que</expan>
                  </choice> dins port </l>

               <l n="11" xml:id="C012-11"> E queus fes venjr en acort </l>

               <l n="12" xml:id="C012-12"> Que diu lesguart enamorat </l>

               <l n="13" xml:id="C012-13"> De mon cor nom puch aiudar </l>

               <l n="14" xml:id="C012-14"> Ne dema lengua per lo dir </l>

               <l n="15" xml:id="C012-15"> Dien ques deu lull atrauir </l>

               <l n="16" xml:id="C012-16"> Car mils sen pot atras tornar </l>
            </lg>
            <lg type="cobla" n="3" xml:id="C012-03">

               <l n="17" xml:id="C012-17"> Sobre vos es tot be justat </l>

               <l n="18" xml:id="C012-18"> Mas feu semblant no voler port </l>

               <l n="19" xml:id="C012-19"> De la merçe quel carrech tort </l>

               <l n="20" xml:id="C012-20"> Ab son bell geny ha redreçat </l>

               <l n="21" xml:id="C012-21"> Qui pot no deu tal fex portar </l>

               <l n="22" xml:id="C012-22"> Volent tal pena soferir </l>

               <l n="23" xml:id="C012-23"> E la merçe noy vol sopplir </l>

               <l n="24" xml:id="C012-24"> Per lo gran carrech parençar </l>
            </lg>
            <lg type="cobla" n="4" xml:id="C012-04">

               <l n="25" xml:id="C012-25"> Qui es <choice>
                     <abbr>aꝗll</abbr>
                     <expan>aquell</expan>
                  </choice> tan mal fadat </l>

               <l n="26" xml:id="C012-26"> Que sens sper li fall conort </l>

               <l n="27" xml:id="C012-27"> E no sesforç contral cas fort </l>

               <l n="28" xml:id="C012-28"> Puix de aquell es desperat </l>

               <l n="29" xml:id="C012-29"> E yo nom puch aconortar </l>

               <l n="30" xml:id="C012-30"> A ns prench tot ginys <choice>
                     <abbr>ꝑ</abbr>
                     <expan>per</expan>
                  </choice> no sentir </l>

               <l n="31" xml:id="C012-31"> Los grans affanys quem sent venir </l>

               <l n="32" xml:id="C012-32"> En part venuts <choice>
                     <abbr>ꝑ</abbr>
                     <expan>per</expan>
                  </choice> no gosar </l>
            </lg>
            <pb n="13r"
               facs="http://www.cervantesvirtual.com/obra-visor/obres-dausias-march-manuscrito--0/html/01f79830-82b2-11df-acc7-002185ce6064_29.html"/>
            <lg type="cobla" n="5" xml:id="C012-05">

               <l n="33" xml:id="C012-33"> No puch recollir lo desgrat </l>

               <l n="34" xml:id="C012-34"> Quem ve de mi sens ferme tort </l>

               <l n="35" xml:id="C012-35"> Fahent menys mal vetx passar mort </l>

               <l n="36" xml:id="C012-36"> A molt mesqui desuenturat </l>

               <l n="37" xml:id="C012-37"> Car nom so mes en assaijar </l>

               <l n="38" xml:id="C012-38"> Desser lo mjls nat en auyr </l>

               <l n="39" xml:id="C012-39"> Perno poder mj en ardir </l>

               <l n="40" xml:id="C012-40"> En pations damor mostrar </l>
            </lg>
            <lg type="tornada" n="6" xml:id="C012-06">

               <l n="41" xml:id="C012-41"> Les dones prech vullen penssar </l>

               <l n="42" xml:id="C012-42"> Cascunab quin sguart la mir </l>

               <l n="43" xml:id="C012-43"> En lo gest me vull enardir </l>

               <l n="44" xml:id="C012-44"> Puys amor ma tolt parlar </l>
            </lg>
         </div>
