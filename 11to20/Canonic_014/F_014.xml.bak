   <text>
      <body>
         <pb n="13r"
            facs="http://www.cervantesvirtual.com/obra-visor/poesias-de-ausias-y-mena-manuscrito--0/html/0165fe84-82b2-11df-acc7-002185ce6064_30.html"/>
         <lg type="poem" xml:id="C014-F014">
            <lg type="cobla" n="1" xml:id="C014-01">
               <l n="1" xml:id="C014-1">Mal uenturos no deu cerquar ventura</l>
               
               <l n="2" xml:id="C014-2">Croar se deu lo front com la nomenen</l>
               
               <l n="3" xml:id="C014-3">No desloant los quj fauor ne prenen</l>
               
               <l n="4" xml:id="C014-4">Entreposant lur bon enginy e cura</l>
               
               <l n="5" xml:id="C014-5">Negu no deu son juy tan prest auer</l>
               
               <l n="6" xml:id="C014-6">Que cell desllou qui uentura uol fenyer</l>
               
               <l n="7" xml:id="C014-7">No uirtuos denegant li atenyer</l>
               
               <l n="8" xml:id="C014-8">Co que sos fills han menys dalgun saber</l>
            </lg>
            
            <lg type="cobla" n="2" xml:id="C014-02">
               <l n="9" xml:id="C014-9">Lleix me donchs desser auenturer</l>
            
               <l n="10" xml:id="C014-10">Si be nom pusch tant retraure nestrenyer</l>
               
               <l n="11" xml:id="C014-11">Quen contra dell me pogues tant empenyer</l>
               
               <l n="12" xml:id="C014-12">Si que fugis ason peu molt lauger</l>
               
               <l n="13" xml:id="C014-13">Senyor es gran bens mundans regidor<del rend="overstrike">s</del></l>
               
               <l n="14" xml:id="C014-14">E son juhy es ab hull amagat</l>
               
               <l n="15" xml:id="C014-15">Fferint uos lla don sereu delitat</l>
               
               <l n="16" xml:id="C014-16">E quel delit uengues tot per/amor</l>
            </lg>
            
            <lg type="cobla" n="3" xml:id="C014-03">
               <l n="17" xml:id="C014-17">Lonch es lo temps del continuu dolor</l>
            
               <l n="18" xml:id="C014-18">A part detras car son cinch anys passats</l>
               
               <l n="19" xml:id="C014-19">Quem fuig delit com hy suy acostats</l>
               
               <l n="20" xml:id="C014-20">Ffent me sentir fret apres de calor</l>
               
               <l n="21" xml:id="C014-21">Diuerssitat de cassos aportant</l>
               
               <l n="22" xml:id="C014-22">Per degastar damor lo benifet</l>
               
               <l n="23" xml:id="C014-23">Dels bens damor conseruador sen ret</l>
               
               <l n="24" xml:id="C014-24">Ffora content yo quj so desijant</l>
            </lg>
            
            <pb n="14r"
               facs="http://www.cervantesvirtual.com/obra-visor/poesias-de-ausias-y-mena-manuscrito--0/html/0165fe84-82b2-11df-acc7-002185ce6064_31.html"/>
            <lg type="cobla" n="4" xml:id="C014-04">
               <l n="25" xml:id="C014-25">A deu nel nom noso damor clamant</l>
            
               <l n="26" xml:id="C014-26">Car be ha dat aygua magranda set</l>
               
               <l n="27" xml:id="C014-27">No sadollat mas compte lin es fet</l>
               
               <l n="28" xml:id="C014-28">No pot complir co queso desijant</l>
               
               <l n="29" xml:id="C014-29">E no mereix algun repreniment</l>
               
               <l n="30" xml:id="C014-30">Car del cel terc eguals forces nos fa</l>
               
               <l n="31" xml:id="C014-31">Segons cascu amar dispost esta</l>
               
               <l n="32" xml:id="C014-32">E tant com pot estreny son manament</l>
            </lg>
            
            <lg type="cobla" n="5" xml:id="C014-05">
               <l n="33" xml:id="C014-33">Fortuit cas ma fet esser absent</l>
            
               <l n="34" xml:id="C014-34">Del foch damor quj jam illumina</l>
               
               <l n="35" xml:id="C014-35">En millor cas ventural apaga</l>
               
               <l n="36" xml:id="C014-36">E yo son cert quamor ne fon dolent</l>
               
               <l n="37" xml:id="C014-37">Perque nom clam damor mas de ventura</l>
               
               <l n="38" xml:id="C014-38">Car son poder me ha desfauorit</l>
               
               <l n="39" xml:id="C014-39">En abando nom uol hauer jaqujt</l>
               
               <l n="40" xml:id="C014-40">Ans persegueix amor /e ma gran cura</l>
            </lg>
            
            <lg type="cobla" n="6" xml:id="C014-06">
               <l n="41" xml:id="C014-41">Lir entre carts del mon deu perdre cura</l>
            
               <l n="42" xml:id="C014-42">Quili coue damor perdresperanca</l>
               
               <l n="43" xml:id="C014-43">E tal o qual es damor la fermanca</l>
               
               <l n="44" xml:id="C014-44">Ab lo desig queli dona punctura</l>
            </lg>
         </lg>
      </body>
