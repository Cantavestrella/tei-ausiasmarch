   <text>
      <body>
         <pb n="87r"
            facs="http://bdh-rd.bne.es/viewer.vm?id=0000149777"/>
         <lg type="poem" xml:id="C014-E057">
            <lg type="cobla" n="1" xml:id="C014-01">
               <l n="1" xml:id="C014-1">Mal venturos / no deu çercar ventura</l>
               
               <l n="2" xml:id="C014-2">crohar se deu / lo front com la nomenen</l>
               
               <l n="3" xml:id="C014-3">no deslloant / çells qui fauor ne prenen</l>
               
               <l n="4" xml:id="C014-4">entre posant / llur bon enginy e cura</l>
               
               <l n="5" xml:id="C014-5">Degu no deu / Juhi tant prest hauer</l>
               
               <l n="6" xml:id="C014-6">que çell desllou / qui ventura vol çenyer</l>
               
               <l n="7" xml:id="C014-7">no virtuos / denegant li attenyer</l>
               
               <l n="8" xml:id="C014-8">ço que sos fills / han menys dalgun saber</l>
            </lg>
            <pb n="87v"
               facs="http://bdh-rd.bne.es/viewer.vm?id=0000149777"/>
            <lg type="cobla" n="2" xml:id="C014-02">
               <l n="9" xml:id="C014-9">E leix me donchs / de ser auenturer</l>
               
               <l n="10" xml:id="C014-10">si be nom pusch / tant retraure ne strenyer</l>
               
               <l n="11" xml:id="C014-11">quen contra luy / me posques tant empenyer</l>
               
               <l n="12" xml:id="C014-12">si que fugis / a son peu molt llauger</l>
               
               <l n="13" xml:id="C014-13"><choice><abbr>Seńor</abbr><expan>Sennor</expan></choice> es gran / degu no es / major</l>
               
               <l n="14" xml:id="C014-14">e son juhi / es al vll amagat</l>
               
               <l n="15" xml:id="C014-15">ferint vos llay / don serets delitat</l>
               
               <l n="16" xml:id="C014-16">e quel delit / vengues tot per amor</l>
            </lg>
            
            <lg type="cobla" n="3" xml:id="C014-03">
               <l n="17" xml:id="C014-17">Lonch es lo temps / del continu dolor</l>
            
               <l n="18" xml:id="C014-18">a part derrer / car son cinch anys passats</l>
               
               <l n="19" xml:id="C014-19">em fuig delit / com hi suy acostats</l>
               
               <l n="20" xml:id="C014-20">fent me sentir / fret apres de calor</l>
               
               <l n="21" xml:id="C014-21">Diuersitat / de casos aportant</l>
               
               <l n="22" xml:id="C014-22">per degastar / damor lo benifet</l>
               
               <l n="23" xml:id="C014-23">dels bens daquell / conseruador sen ret</l>
               
               <l n="24" xml:id="C014-24">fora content / çell qui es desijant</l>
            </lg>
            
            <lg type="cobla" n="4" xml:id="C014-04">
               <l n="25" xml:id="C014-25">A Deu nel mon / nosuy damor clamant</l>
            
               <l n="26" xml:id="C014-26">car be ha dat / aygua ma granda set</l>
               
               <l n="27" xml:id="C014-27">no sadollat / mas compte mi nesfet</l>
               
               <l n="28" xml:id="C014-28">nos pot complir / ço que sa <add place="above">uy</add> sedejant</l>
               
               <l n="29" xml:id="C014-29"> E no mereix / alguns repreniments</l>
               
               <l n="30" xml:id="C014-30">car del çel terç / eguals forçes nos fa</l>
               
               <l n="31" xml:id="C014-31">segons cascu / amar dispost esta</l>
               
               <l n="32" xml:id="C014-32">e tant com pot / estreny sos manaments</l>
            </lg>
           
            <pb n="88r"
               facs="http://bdh-rd.bne.es/viewer.vm?id=0000149777"/>
            <lg type="cobla" n="5" xml:id="C014-05">
               <l n="33" xml:id="C014-33">Fortuhit cas / ma fet esser absents</l>
            
               <l n="34" xml:id="C014-34">del foch damor / que jam jllumina</l>
               
               <l n="35" xml:id="C014-35">en millor cas / ventura lapaga</l>
               
               <l n="36" xml:id="C014-36">e yo somçert / quamor ne fonch dolents</l>
               
               <l n="37" xml:id="C014-37">Per que nom clam / damor mas de ventura</l>
               
               <l n="38" xml:id="C014-38">car son poder / me ha desfauorit</l>
               
               <l n="39" xml:id="C014-39">en abando / nom vol hauer jaquit</l>
               
               <l n="40" xml:id="C014-40">ans persegueix / amor a ma gran cura</l>
            </lg>
            
            <lg type="cobla" n="6" xml:id="C015-06">
               <l n="41" xml:id="C014-41">Lir entre carts / del mon deu perdre cura</l>
            
               <l n="42" xml:id="C014-42">qui li coue / damor perdre sperança</l>
               
               <l n="43" xml:id="C014-43">e tal e qual / es damor la fermança</l>
               
               <l n="44" xml:id="C014-44">ab lo desig / qui li dona punctura</l>
            </lg>
         </lg>
