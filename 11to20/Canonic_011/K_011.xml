   <text>
      <body>
         <div type="poem" xml:id="C011-K011">
            <pb n="11v"
               facs="http://www.cervantesvirtual.com/obra-visor/les-obres-de-mossen-ausias-march-sont-acabada-de-scriure-la-present-obra-per-m-pere-vilaraso-prevere-per-manament-de-lilltre-sor-almirant-de-napols-manuscrit--0/html/00488f44-82b2-11df-acc7-002185ce6064_29.html"/>
            <lg type="cobla" n="1" xml:id="C011-01">

               <l n="1" xml:id="C011-1"> q Uins tan segus <choice>
                     <abbr>cȏçells</abbr>
                     <expan>conçells</expan>
                  </choice> vatg <choice>
                     <abbr>enşcȃ</abbr>
                     <expan>ensercant</expan>
                  </choice>
               </l>

               <l n="2" xml:id="C011-2"> C os malestruch enfastijat de viure </l>

               <l n="3" xml:id="C011-3"> Amich de plor e desamich de riure </l>

               <l n="4" xml:id="C011-4"> Com soferras los mals quit son <choice>
                     <abbr>dauȃt</abbr>
                     <expan>dauant</expan>
                  </choice>
               </l>
               <pb n="12r"
                  facs="http://www.cervantesvirtual.com/obra-visor/les-obres-de-mossen-ausias-march-sont-acabada-de-scriure-la-present-obra-per-m-pere-vilaraso-prevere-per-manament-de-lilltre-sor-almirant-de-napols-manuscrit--0/html/00488f44-82b2-11df-acc7-002185ce6064_30.html"/>
               <l n="5" xml:id="C011-5"> Acuitat donchs ala mort quit spera </l>

               <l n="6" xml:id="C011-6"> E per tos mals te allongues los jorns </l>

               <l n="7" xml:id="C011-7"> Aytan es <choice>
                     <abbr>lȗy</abbr>
                     <expan>luny</expan>
                  </choice> ton delitos soiorns </l>

               <l n="8" xml:id="C011-8"> Com vols fugir ala mort falaguera </l>
            </lg>
            <lg type="cobla" n="2" xml:id="C011-02">

               <l n="9" xml:id="C011-9"> Braços vberts es exida correra </l>

               <l n="10" xml:id="C011-10"> Plorant sos vlls per sobres de <choice>
                     <abbr>grȃ</abbr>
                     <expan>gran</expan>
                  </choice> gotx </l>

               <l n="11" xml:id="C011-11"> Melodios cantar de sa veu otx </l>

               <l n="12" xml:id="C011-12"> Dient amich ix de casastraniera <note type="editorial"
                     >Sembla un italianisme.</note>
               </l>

               <l n="13" xml:id="C011-13"> En delit prench donarte ma fauor </l>

               <l n="14" xml:id="C011-14"> Que per null temps home nat la sentida </l>

               <l n="15" xml:id="C011-15"> Car jo defutx atot home quim crida </l>

               <l n="16" xml:id="C011-16"> Prenent <choice>
                     <abbr>aꝗll</abbr>
                     <expan>aquell</expan>
                  </choice> qui futx de ma rigor </l>
            </lg>
            <lg type="cobla" n="3" xml:id="C011-03">

               <l n="17" xml:id="C011-17"> Ab vlls plorants e cara de terror </l>

               <l n="18" xml:id="C011-18"> Cabells rompent ab <choice>
                     <abbr>grȃs</abbr>
                     <expan>grans</expan>
                  </choice>
                  <choice>
                     <abbr>hudolamȇts</abbr>
                     <expan>hudolaments</expan>
                  </choice>
               </l>

               <l n="19" xml:id="C011-19"> Lauidam vol donar heretaments </l>

               <l n="20" xml:id="C011-20"> E <choice>
                     <abbr>daꝗsts</abbr>
                     <expan>daquests</expan>
                  </choice> dons vol <choice>
                     <abbr>ꝗ</abbr>
                     <expan>que</expan>
                  </choice> sia <choice>
                     <abbr>señor</abbr>
                     <expan>senyor</expan>
                  </choice>
               </l>

               <l n="21" xml:id="C011-21"> Cridant ab veu orrible y dolorosa </l>

               <l n="22" xml:id="C011-22"> Tal com lamort cridal benuyrat </l>

               <l n="23" xml:id="C011-23"> Car si lom es amals aparellat </l>

               <l n="24" xml:id="C011-24"> La veu de mort li es melodiosa </l>
            </lg>
            <lg type="cobla" n="4" xml:id="C011-04">

               <l n="25" xml:id="C011-25"> Bem marauell com es tan ergullosa </l>

               <l n="26" xml:id="C011-26"> La <choice>
                     <abbr>volȗtat</abbr>
                     <expan>voluntat</expan>
                  </choice> de <choice>
                     <abbr>cascȗ</abbr>
                     <expan>cascun</expan>
                  </choice> amador </l>

               <l n="27" xml:id="C011-27"> No demanant amj qui es amor </l>
               <pb n="12v"
                  facs="http://www.cervantesvirtual.com/obra-visor/les-obres-de-mossen-ausias-march-sont-acabada-de-scriure-la-present-obra-per-m-pere-vilaraso-prevere-per-manament-de-lilltre-sor-almirant-de-napols-manuscrit--0/html/00488f44-82b2-11df-acc7-002185ce6064_31.html"/>
               <l n="28" xml:id="C011-28"> En mi sabran sa força poderosa </l>

               <l n="29" xml:id="C011-29"> Tots mal dient <choice>
                     <abbr>sagramȇtaiarȃ</abbr>
                     <expan>sagramentaiaran</expan>
                  </choice>
               </l>

               <l n="30" xml:id="C011-30"> Que may amor los tendran sonpoder </l>

               <l n="31" xml:id="C011-31"> E sils recont lacolorat plaer </l>

               <l n="32" xml:id="C011-32"> Lo temps perdut <choice>
                     <abbr>sospirȃt</abbr>
                     <expan>sospirant</expan>
                  </choice> mal <choice>
                     <abbr>dirȃ</abbr>
                     <expan>diran</expan>
                  </choice>
               </l>
            </lg>
            <lg type="cobla" n="5" xml:id="C011-05">

               <l n="33" xml:id="C011-33"> Null hom conech o donan <choice>
                     <abbr>mȏ</abbr>
                     <expan>mon</expan>
                  </choice>
                  <choice>
                     <abbr>semblȃ</abbr>
                     <expan>semblan</expan>
                  </choice>
               </l>

               <l n="34" xml:id="C011-34"> Que dolorit per amor faça planyer </l>

               <l n="35" xml:id="C011-35"> Yo fon <choice>
                     <abbr>aꝗll</abbr>
                     <expan>aquell</expan>
                  </choice> quis deu hom <choice>
                     <abbr>cȏplȃyer</abbr>
                     <expan>complanyer</expan>
                  </choice>
               </l>

               <l n="36" xml:id="C011-36"> Car de <choice>
                     <abbr>mȏ</abbr>
                     <expan>mon</expan>
                  </choice> cor la sanch sen <choice>
                     <abbr>valonyȃt<del rend="overstrike">t</del></abbr>
                     <expan>valonyan<del rend="overstrike">t</del></expan>
                  </choice>
               </l>


               <l n="37" xml:id="C011-37"> Per gran tristor <choice>
                     <abbr>ꝗ</abbr>
                     <expan>que</expan>
                  </choice> li es acostada </l>

               <l n="38" xml:id="C011-38"> Secas tot jorn lumjt quim soste vida </l>

               <l n="39" xml:id="C011-39"> Ela tristor es contra mj ardida </l>

               <l n="40" xml:id="C011-40"> En mon socos mas no si trobarmada </l>
            </lg>
            <lg type="tornada" n="6" xml:id="C011-06">

               <l n="41" xml:id="C011-41"> Lir entre carts lora sent acostada </l>

               <l n="42" xml:id="C011-42"> Que ciuilment es ma vida finida </l>

               <l n="43" xml:id="C011-43"> Puys que del tot <choice>
                     <abbr>masperȃças</abbr>
                     <expan>masperanças</expan>
                  </choice> fogida </l>

               <l n="44" xml:id="C011-44"> Marma roman en <choice>
                     <abbr>aꝗst</abbr>
                     <expan>aquest</expan>
                  </choice>
                  <choice>
                     <abbr>mȏ</abbr>
                     <expan>mon</expan>
                  </choice> dampnada </l>
            </lg>
         </div>
      </body>
   </text>
