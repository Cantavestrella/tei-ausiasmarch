    <text>
        <body>                                                
        
        <lg type="poem" xml:id="C007-M006">
        <pb n="184r" 
            facs="http://www.cervantesvirtual.com/obra-visor/cancionero-catalan-del-ateneu-ba1-manuscrit--0/html/01e69dd2-82b2-11df-acc7-002185ce6064_371.html"/>
            <lg type="cobla" n="1" xml:id="C0007-1">
                <l n="1" xml:id="C007-1"> Si com ricat<del rend="overstrike">r</del> / no porte bens ab si</l>
                <l n="2" xml:id="C007-2"> Mes val aytant / com cell qujnes senyor</l>
                <l n="3" xml:id="C007-3"> Amor no val mes tant quant lamador</l>
                <l n="4" xml:id="C007-4"> Manxa bufant / orga fals noret fj</l>
                <l n="5" xml:id="C007-5"> Amor val poch / com tot anemorat</l>
                <l n="6" xml:id="C007-6"> Haffal sedat / djns son pits fals enclosa</l>
                <l n="7" xml:id="C007-7"> Oes ligat / ab vna tal sposa</l>
                <l n="8" xml:id="C007-8"> Pag\interlin{u}esa es / lo dret nom apellat</l>
                </lg>
            <lg type="cobla" n="2" xml:id="C0007-2">
                <l n="9" xml:id="C007-9"> Amor no pot / auer desordonat</l>
                <l n="10" xml:id="C007-10"> Co quedeufa / natura mit gensant</l>
                <l n="11" xml:id="C007-11"> Car home pech / no poch sser fin amant</l>
                <l n="12" xml:id="C007-12"> Ne sobtil jens / contra sa qualita</l>
                <l n="13" xml:id="C007-13"> Mal pendra pjnt / en laygua safjgura</l>
                <l n="14" xml:id="C007-14"> Molt meyns amor / pendra lono djspost</l>
                <l n="15" xml:id="C007-15"> No pot estar / layga djns vnloch rost</l>
                <l n="16" xml:id="C007-16"> Tempoch amor / encap dom foll atur</l>
                </lg>
            <lg type="cobla" n="3" xml:id="C0007-3">
                <l n="17" xml:id="C007-17"> Per ben amar / ab engoxosa cura</l>
                <l n="18" xml:id="C007-18"> En temps passat / erem ladonchs dolguts</l>
                <l n="19" xml:id="C007-19"> Ouidil prous / dir quamor es crescuts</l>
                <l n="20" xml:id="C007-20"> Per altramor / demostrant safactura</l>
                <l n="21" xml:id="C007-21"> Verdader fonch / sos dits esos prehichs</l>
                <l n="22" xml:id="C007-22"> Tant quant amor / fonch prop <choice><abbr>ƌ</abbr><expan>de</expan></choice> <choice><abbr>conaxȇça</abbr><expan>conaxença</expan></choice></l>
                <l n="23" xml:id="C007-23"> Mes aquest cars / entrels amal dolenta</l>
                <l n="24" xml:id="C007-24"> Tant que no creu / null temps sien amjchs</l>
                </lg>
        <pb n="184v" 
            facs="http://www.cervantesvirtual.com/obra-visor/cancionero-catalan-del-ateneu-ba1-manuscrit--0/html/01e69dd2-82b2-11df-acc7-002185ce6064_372.html"/> 
            <lg type="cobla" n="4" xml:id="C0007-4">
                <l n="25" xml:id="C007-25"> Siffossem nats / vos ejontrels antjchs</l>
                <l n="26" xml:id="C007-26"> Layn quant amor / amant se conquerja</l>
                <l n="27" xml:id="C007-27"> Sens praticar / alguna manestrja</l>
                <l n="28" xml:id="C007-28"> Lo vostre cor / no fora tan jnjchs</l>
                <l n="29" xml:id="C007-29"> En vos conech / gran disposicio</l>
                <l n="30" xml:id="C007-30"> Deffer tot so / que jentilesa mana</l>
                <l n="31" xml:id="C007-31"> Mes crjament / veig <choice><abbr>ꝗ</abbr><expan>que</expan></choice> naturangana</l>
                <l n="32" xml:id="C007-32"> Car viurabells / es dom <choice><abbr>ꝑdicio</abbr><expan>perdicio</expan></choice></l>
                </lg>
            <lg type="cobla" n="5" xml:id="C0007-5">
                <l n="33" xml:id="C007-33"> Per mal grair / ne per mala saho</l>
                <l n="34" xml:id="C007-34"> Moncor no pot / amor desemperar</l>
                <l n="35" xml:id="C007-35"> Deuotament / los meplau remembrar</l>
                <l n="36" xml:id="C007-36"> Aquells passats / aquj deus don <choice><abbr>ꝑdo</abbr><expan>perdo</expan></choice></l>
                <l n="37" xml:id="C007-37"> E com sere / trespessat <choice><abbr>daꝗst</abbr><expan>daquest</expan></choice> mon</l>
                <l n="38" xml:id="C007-38"> Letres djran / sobre lamja tomba</l>
                <l n="39" xml:id="C007-39"> Plenade seny / no tjngau agran bomba</l>
                <l n="40" xml:id="C007-40"> Com per vos mort / so e vaig no se hon</l>
                </lg>
            <lg type="cobla" n="6" xml:id="C0007-6">
                <l n="41" xml:id="C007-41"> Enveyes tal / quetot prjmer confon</l>
                <l n="42" xml:id="C007-42"> En tots aquells / quj ensi la sagustan</l>
                <l n="43" xml:id="C007-43"> Los envejats / hun poch nemolt no gustan</l>
                <l n="44" xml:id="C007-44"> Aquell mal tast / qujls envegosos fon</l>
                <l n="45" xml:id="C007-45"> Tal es amor / carjo quj lame just</l>
                <l n="46" xml:id="C007-46"> Sens gran dolor / dant me forts moujments</l>
                <l n="47" xml:id="C007-47"> E   adeu / daço tals sentjments</l>
                <l n="48" xml:id="C007-48"> Com fort destral / adetallar molt fust</l>
                </lg>
        <pb n="185r" 
            facs="http://www.cervantesvirtual.com/obra-visor/cancionero-catalan-del-ateneu-ba1-manuscrit--0/html/01e69dd2-82b2-11df-acc7-002185ce6064_373.html"/> 
            <lg type="cobla" n="7" xml:id="C0007-7">
                <l n="49" xml:id="C007-49"> Si com adam / pres mal del vedat gust</l>
                <l n="50" xml:id="C007-50"> Com samuller / li mostra mal camj</l>
                <l n="51" xml:id="C007-51"> Dient adam / menje daquest bocj</l>
                <l n="52" xml:id="C007-52"> Essemblarem / adeu quj es tot just</l>
                <l n="53" xml:id="C007-53"> Nepren amj / car mon seyn acregut</l>
                <l n="54" xml:id="C007-54"> La voluntat / fent li promjsio</l>
                <l n="55" xml:id="C007-55"> Que be serujnt / aconsegriado</l>
                <l n="56" xml:id="C007-56"> Que <choice><abbr>ꝑ</abbr><expan>per</expan></choice> null temps / talno fo conegut</l>
                </lg>
            <lg type="cobla" n="8" xml:id="C0007-8">
                <l n="57" xml:id="C007-57"> Per mal servir / no crech aje <choice><abbr>ꝑdut</abbr><expan>perdut</expan></choice></l>
                <l n="58" xml:id="C007-58"> Car sils treballs / agues soferts <choice><abbr>ꝑ</abbr><expan>per</expan></choice> deu</l>
                <l n="59" xml:id="C007-59"> Cars glorios / foren lo Regne seu</l>
                <l n="60" xml:id="C007-60"> Eja plorant / soujnt metrobat mut</l>
                <l n="61" xml:id="C007-61"> Sim fos donat / <choice><abbr>aꝗst</abbr><expan>aquest</expan></choice> <choice><abbr>tȇps</abbr><expan>temps</expan></choice> en entendre</l>
                <l n="62" xml:id="C007-62"> Los grans sacrets / enclosos en natura</l>
                <l n="63" xml:id="C007-63"> Nom foral mon / cosa qujm fos escura</l>
                <l n="64" xml:id="C007-64"> Dels fets djujns / gran part nepogren tendre</l>
                </lg>
            <lg type="tornada">
                <l n="65" xml:id="C007-65"> Plena deseyn / tot mon temps vul <choice><abbr>despȇdre</abbr><expan>despendre</expan></choice></l>
                <l n="66" xml:id="C007-66"> Amant auos / sens algun grau consegre</l>
                <l n="67" xml:id="C007-67"> Edurara / ffins que del Riu de Segre</l>
                <l n="68" xml:id="C007-68"> Laygua corent / <choice><abbr>amŭt</abbr><expan>amunt</expan></choice> sepuguestendre</l>
                </lg>
              </lg>
        </body>
    </text> 
