    <text>
        <body>
            <div type="poem" xml:id="C007-L002">
                <pb n="106r" facs="https://mdc.csuc.cat/digital/collection/manuscritBC/id/272071"/>
                <lg type="cobla" n="1" xml:id="C007-lg1">
                    <l n="1" xml:id="C007-l1"> Si com rictat / no porte bens ab si</l>
                    <l n="2" xml:id="C007-l2"> Mes val aytant / com cell quin es senyor</l>
                    <l n="3" xml:id="C007-l3"> Amor no val / mes tant com lamador</l>
                    <l n="4" xml:id="C007-l4"> Manxa buffant / orga fals no ret fi</l>
                    <l n="5" xml:id="C007-l5"> Amor val poch / com tot enamorat</l>
                    <l n="6" xml:id="C007-l6"> Ha falsetat / dins son pits fals enclosa</l>
                    <l n="7" xml:id="C007-l7"> O es ligat / ab vna tal sposa</l>
                    <l n="8" xml:id="C007-l8"> Paguesa es / lo dret nom apellat</l>
                </lg>
                <lg type="cobla" n="2" xml:id="C007-lg2">
                    <l n="9" xml:id="C007-l9"> Amor no pot / hauer desordonat</l>
                    <l n="10" xml:id="C007-l10"> Ço que deu fa / natura migensant</l>
                    <l n="11" xml:id="C007-l11"> Car home pech / no poch ser finamant</l>
                    <pb n="106v"
                        facs="https://mdc.csuc.cat/digital/collection/manuscritBC/id/272072"/>
                    <l n="12" xml:id="C007-l12"> Ne soptil gens / contra sa qualitat</l>
                    <l n="13" xml:id="C007-l13"> Mal pendra pint / en layga se figura</l>
                    <l n="14" xml:id="C007-l14"> Molt menys amor / pendre lo no dispost</l>
                    <l n="15" xml:id="C007-l15"> No pot star / layga dins vn loch rost</l>
                    <l n="16" xml:id="C007-l16"> Ten poch amor / en cap dom foll atura</l>
                </lg>
                <lg type="cobla" n="3" xml:id="C007-lg3">
                    <l n="17" xml:id="C007-l17"> Per ben amar / am angoxasa cura</l>
                    <l n="18" xml:id="C007-l18"> En temps passat / erem ladonchs volguts</l>
                    <l n="19" xml:id="C007-l19"> Ouidil <choice>
                            <abbr>proꝯ</abbr>
                            <expan>prous</expan>
                        </choice> / dix quamor es crescuts</l>
                    <l n="20" xml:id="C007-l20"> Per altreamor / demostrant sa factura</l>
                    <l n="21" xml:id="C007-l21"> Vertader fonch / sos dits e sos prehichs</l>
                    <l n="22" xml:id="C007-l22"> Tant quant amor / fonch <choice>
                            <abbr>ṗp</abbr>
                            <expan>prop</expan>
                        </choice> de conexença</l>
                    <l n="23" xml:id="C007-l23"> Mes aquest cars / entrells ha mal volença</l>
                    <l n="24" xml:id="C007-l24"> Tant <choice>
                            <abbr>ꝗ</abbr>
                            <expan>que</expan>
                        </choice> no creu /null temps sien amichs</l>
                </lg>
                <lg type="cobla" n="4" xml:id="C007-lg4">
                    <l n="25" xml:id="C007-l25"> Si fossem nats / vos e yontrels antichs</l>
                    <l n="26" xml:id="C007-l26"> Lany <choice>
                            <abbr>quȃt</abbr>
                            <expan>quant</expan>
                        </choice> amor / amant se encorria</l>
                    <l n="27" xml:id="C007-l27"> Sens praticar / alguna manestria</l>
                    <l n="28" xml:id="C007-l28"> Lo vostre cor / no fora tan jnichs</l>
                    <l n="29" xml:id="C007-l29"> En vos conech / gran disposicio</l>
                    <l n="30" xml:id="C007-l30"> De fer tot ço / que gentilesa mana</l>
                    <l n="31" xml:id="C007-l31"> Mes <choice>
                            <abbr>criamȇt</abbr>
                            <expan>criament</expan>
                        </choice> / veig <choice>
                            <abbr>ꝗ</abbr>
                            <expan>que</expan>
                        </choice> naturangana</l>
                    <l n="32" xml:id="C007-l32"> Car viurabells / es dom perdicio</l>
                </lg>
                <pb n="107r" facs="https://mdc.csuc.cat/digital/collection/manuscritBC/id/272073"/>
                <lg type="cobla" n="5" xml:id="C007-lg5">
                    <l n="33" xml:id="C007-l33"> Per mal grahir / ne <choice>
                            <abbr>ꝑ</abbr>
                            <expan>per</expan>
                        </choice> mala saho</l>
                    <l n="34" xml:id="C007-l34"> Mon cor no pot / amor desemperar</l>
                    <l n="35" xml:id="C007-l35"> Deuotament / los me plau remembrar</l>
                    <l n="36" xml:id="C007-l36"> Aquells passats / aquj <choice>
                            <abbr>deꝯ</abbr>
                            <expan>deus</expan>
                        </choice> don perdo</l>
                    <l n="37" xml:id="C007-l37"> E com sere / traspessat daquest mon</l>
                    <l n="38" xml:id="C007-l38"> Letres diran / sobre la mia tomba</l>
                    <l n="39" xml:id="C007-l39"> Plena de seny / no tingau agran bomba</l>
                    <l n="40" xml:id="C007-l40"> Com per vos <del rend="overstrike">mort</del><add
                            place="above">muyr</add> / <del rend="overstrike">so</del> e vaig no se
                        hon /</l>
                </lg>
                <lg type="cobla" n="6" xml:id="C007-lg6">
                    <l n="41" xml:id="C007-l41"> Enueges tal / que tot primer confon</l>
                    <l n="42" xml:id="C007-l42"> A tots <choice>
                            <abbr>aꝗlls</abbr>
                            <expan>aquells</expan>
                        </choice> / quj en si la segusten</l>
                    <l n="43" xml:id="C007-l43"> Los enuyats / vn poch nj molt no gusten</l>
                    <l n="44" xml:id="C007-l44"> Aquell mal tast / qujls enuegesos fon</l>
                    <l n="45" xml:id="C007-l45"> Tal es amor / car yo quj lame just</l>
                    <l n="46" xml:id="C007-l46"> Sens grans dolors / dant me fort <choice>
                            <abbr>mouimȇts</abbr>
                            <expan>mouiments</expan>
                        </choice></l>
                    <l n="47" xml:id="C007-l47"> E adeu / daço tals sentiments</l>
                    <l n="48" xml:id="C007-l48"> Com fort destral / ha detallar molt fust</l>
                </lg>
                <lg type="cobla" n="7" xml:id="C007-lg7">
                    <l n="49" xml:id="C007-l49"> Si com Adam / pres mal del vedat gust</l>
                    <l n="50" xml:id="C007-l50"> Com sa muller / li mostra mal camj</l>
                    <l n="51" xml:id="C007-l51"> Dient Adam / <choice>
                            <abbr>mengȇ</abbr>
                            <expan>mengem</expan>
                        </choice> daquest boci</l>
                    <l n="52" xml:id="C007-l52"> E semblarem / adeu quj es tot just</l>
                    <pb n="107v"
                        facs="https://mdc.csuc.cat/digital/collection/manuscritBC/id/272074"/>
                    <l n="53" xml:id="C007-l53"> Nepren amj / car mon seny ha cregut</l>
                    <l n="54" xml:id="C007-l54"> La voluntat / fent li promissio</l>
                    <l n="55" xml:id="C007-l55"> Que be <choice>
                            <abbr>şujnt</abbr>
                            <expan>serujnt</expan>
                        </choice> / aconseguira do</l>
                    <l n="56" xml:id="C007-l56"> Que <choice>
                            <abbr>ꝑ</abbr>
                            <expan>per</expan>
                        </choice> null temps / tal no fo conegut</l>
                </lg>
                <lg type="cobla" n="8" xml:id="C007-lg8">
                    <l n="57" xml:id="C007-l57"> Per mal <choice>
                            <abbr>şujr</abbr>
                            <expan>serujr</expan>
                        </choice> / no crech haia <choice>
                            <abbr>ꝑdut</abbr>
                            <expan>perdut</expan>
                        </choice></l>
                    <l n="58" xml:id="C007-l58"> Car sils treballs / hagues sofferts <choice>
                            <abbr>ꝑdeu</abbr>
                            <expan>perdeu</expan>
                        </choice></l>
                    <l n="59" xml:id="C007-l59"> Cors glorios / foran lo Regne seu</l>
                    <l n="60" xml:id="C007-l60"> E ja plorant / souint me trobat mut</l>
                    <l n="61" xml:id="C007-l61"> Sim fos donat / <choice>
                            <abbr>aꝗst</abbr>
                            <expan>aquest</expan>
                        </choice> temps en <choice>
                            <abbr>entȇdre</abbr>
                            <expan>entendre</expan>
                        </choice></l>
                    <l n="62" xml:id="C007-l62"> Los grans secrets / enclosos en natura</l>
                    <l n="63" xml:id="C007-l63"> No foralmon / cosa quim fos scura</l>
                    <l n="64" xml:id="C007-l64"> Dels fets diujns / gran part ne pogrentendre</l>
                </lg>
                <lg type="tornada" n="9" xml:id="C007-lg9">
                    <l n="65" xml:id="C007-l65"> Plena de seny / tot <choice>
                            <abbr>mȏ</abbr>
                            <expan>mon</expan>
                        </choice>
                        <choice>
                            <abbr>tȇps</abbr>
                            <expan>temps</expan>
                        </choice> vull despendre</l>
                    <l n="66" xml:id="C007-l66"> Amant a vos / sens algun grat consegre</l>
                    <l n="67" xml:id="C007-l67"> E durara / fins que del riu de Segre</l>
                    <l n="68" xml:id="C007-l68"> Layga corrent / <choice>
                            <abbr>amȗt</abbr>
                            <expan>amunt</expan>
                        </choice> sepuga stendre</l>
                </lg>
            </div>
        </body>
