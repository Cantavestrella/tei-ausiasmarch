    <text>
        <body>
            <pb n="6r"
                facs="http://www.cervantesvirtual.com/obra-visor/poetas-catalanes-manuscrito-biblioteca-de-la-universidad-de-zaragoza-manuscrit--0/html/01660e74-82b2-11df-acc7-002185ce6064_6.html"/>
            <div type="poem" xml:id="C008-H002">
                <lg type="cobla" n="1" xml:id="C0008-1">
                    <l n="1" xml:id="C008-1"> Ia tots mos cants me plau metrenoblit</l>
                    <l n="2" xml:id="C008-2"> Fforagitant mon gentill pensament</l>
                    <l n="3" xml:id="C008-3"> E ffinamor / demjs pertra breument</l>
                    <l n="4" xml:id="C008-4"> E si com falç drut / sercare delljt</l>
                    <l n="5" xml:id="C008-5"> Axis conquer / en aquest <choice>
                            <abbr>ŧmps</abbr>
                            <expan>temps</expan>
                        </choice> aymia</l>
                    <l n="6" xml:id="C008-6"> Cobbles e lays dansas e bon saber</l>
                    <l n="7" xml:id="C008-7"> Lo dret damor / no podeu conquerer</l>
                    <l n="8" xml:id="C008-8"> Pasa lo temps / <choice>
                            <abbr>ꝗll</abbr>
                            <expan>quell</expan>
                        </choice> bo fauor auja</l>
                </lg>
                <lg type="cobla" n="2" xml:id="C0008-2">
                    <l n="9" xml:id="C008-9"> Segirell <choice>
                            <abbr>ŧmps</abbr>
                            <expan>temps</expan>
                        </choice> / ab affanyos despit</l>
                    <l n="10" xml:id="C008-10"> Si coll dansant segueix al <choice>
                            <abbr>sturmȇt</abbr>
                            <expan>sturment</expan>
                        </choice>t</l>
                    <l n="11" xml:id="C008-11"> E mostra be auer poch sentiment</l>
                    <l n="12" xml:id="C008-12"> Si per hun temps dança rustit bulljt</l>
                    <l n="13" xml:id="C008-13"> En temps passat mudança no sentia</l>
                    <l n="14" xml:id="C008-14"> Daçom reprench em tinch molt <choice>
                            <abbr>ꝑ</abbr>
                            <expan>per</expan>
                        </choice> grosser</l>
                    <l n="15" xml:id="C008-15"> Cuydant saber tot <choice>
                            <abbr>ꝗnt</abbr>
                            <expan>quant</expan>
                        </choice> era mester</l>
                    <l n="16" xml:id="C008-16"> Ffoll es perfet qujs veu menys <choice>
                            <abbr>ƌ</abbr>
                            <expan>de</expan>
                        </choice> follja</l>
                </lg>
                <pb n="6v"
                    facs="http://www.cervantesvirtual.com/obra-visor/poetas-catalanes-manuscrito-biblioteca-de-la-universidad-de-zaragoza-manuscrit--0/html/01660e74-82b2-11df-acc7-002185ce6064_7.html"/>
                <lg type="cobla" n="3" xml:id="C0008-3">
                    <l n="17" xml:id="C008-17"> Si com linfant <choice>
                            <abbr>ꝗ</abbr>
                            <expan>qui</expan>
                        </choice> tem mall sperit</l>
                    <l n="18" xml:id="C008-18"> Com li deffall compayia <choice>
                            <abbr>ƌ</abbr>
                            <expan>de</expan>
                        </choice> gent</l>
                    <l n="19" xml:id="C008-19"> Pren<add place="above">ia</add> mj quj dubtauell
                        turment</l>
                    <l n="20" xml:id="C008-20"> Quem dauamor acostant se la njt</l>
                    <l n="21" xml:id="C008-21"> E desitge ço queser no podia</l>
                    <l n="22" xml:id="C008-22"> Car ffermetat en ell no por auer</l>
                    <l n="23" xml:id="C008-23"> Puys no es pus <choice>
                            <abbr>ꝗ</abbr>
                            <expan>que</expan>
                        </choice> destemprat uoler</l>
                    <l n="24" xml:id="C008-24"> E dura tant com la pasioll guja</l>
                </lg>
                <lg type="cobla" n="4" xml:id="C0008-4">
                    <l n="25" xml:id="C008-25"> Si com <choice>
                            <abbr>aꝗll</abbr>
                            <expan>aquell</expan>
                        </choice> qujstal bosch escondit</l>
                    <l n="26" xml:id="C008-26"> Robant les gents / matant lo deffenent</l>
                    <l n="27" xml:id="C008-27"> E cuydesser / adeu humjll <choice>
                            <abbr>şuent</abbr>
                            <expan>seruent</expan>
                        </choice></l>
                    <l n="28" xml:id="C008-28"> Ffaent retret <choice>
                            <abbr>ƌl</abbr>
                            <expan>del</expan>
                        </choice> temps quel ha <choice>
                            <abbr>şujt</abbr>
                            <expan>serujt</expan>
                        </choice></l>
                    <l n="29" xml:id="C008-29"> Ne pres amj / que vos amor seruja</l>
                    <l n="30" xml:id="C008-30"> Passant affanys sperant lo plaer</l>
                    <l n="31" xml:id="C008-31"> Amant molt fferm ab hun scur esper</l>
                    <l n="32" xml:id="C008-32"> Puys he peccat / yo meresch punjt sia</l>
                </lg>
                <lg type="cobla" n="5" xml:id="C0008-5">
                    <l n="33" xml:id="C008-33"> Molt me reprench / com damor he mal dit</l>
                    <l n="34" xml:id="C008-34"> Ne dire mall <choice>
                            <abbr>ƌ</abbr>
                            <expan>de</expan>
                        </choice> donalmon viuent</l>
                    <l n="35" xml:id="C008-35"> E si lo soll es calt naturalment</l>
                    <l n="36" xml:id="C008-36"> Si no es fret no deu ser corregit</l>
                    <l n="37" xml:id="C008-37">
                        <choice>
                            <abbr>ꝑque</abbr>
                            <expan>Perque</expan>
                        </choice> damor io mal ia no dirja</l>
                    <l n="38" xml:id="C008-38"> Quen ell no es de ben fer lo poder</l>
                    <l n="39" xml:id="C008-39"> Car ffermetat de donayes mester</l>
                    <l n="40" xml:id="C008-40"> Si la vahes <choice>
                            <abbr>ꝑ</abbr>
                            <expan>per</expan>
                        </choice> deu ladorarja</l>
                </lg>
                <lg type="tornada">
                    <l n="41" xml:id="C008-41"> Per sa bondat prech la verge marja</l>
                    <l n="42" xml:id="C008-42"> Quen son serujr / cambie <choice>
                            <abbr>mȏ</abbr>
                            <expan>mon</expan>
                        </choice> voler</l>
                    <l n="43" xml:id="C008-43"> Mostrant me clar com han perdut carrer</l>
                    <l n="44" xml:id="C008-44"> Los <choice>
                            <abbr>ꝗ</abbr>
                            <expan>qui</expan>
                        </choice> amor <choice>
                            <abbr>ƌlas</abbr>
                            <expan>delas</expan>
                        </choice> donas han via</l>
                </lg>
            </div>
        </body>
    </text>
