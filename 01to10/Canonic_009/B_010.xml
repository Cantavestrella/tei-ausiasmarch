    <text>
        <body>
            <pb n="10r"
                facs="http://www.cervantesvirtual.com/obra-visor/obres-dausias-march-manuscrito--0/html/01f79830-82b2-11df-acc7-002185ce6064_23.html"/>
            <div type="poem" xml:id="C009-B010">
                <lg type="cobla" n="1" xml:id="C0009-1">
                    <l n="1" xml:id="C009-1"> aMor se dol com breument yo no muyr</l>
                    <l n="2" xml:id="C009-2"> Pus no li fall per esser de mj fart</l>
                    <l n="3" xml:id="C009-3"> Car sos mals ginys ma portat en tal part</l>
                    <l n="4" xml:id="C009-4"> Que mon delit es quant lo plor abuyr</l>
                    <l n="5" xml:id="C009-5"> E de mon dan yo no so mal mirent</l>
                    <l n="6" xml:id="C009-6"> Car so forçat entrar dins tal preso</l>
                    <l n="7" xml:id="C009-7"> Quel seny tinch pres larbitrey la raho</l>
                    <l n="8" xml:id="C009-8"> Amor o te per seu forçadament.</l>
                </lg>
                <lg type="cobla" n="2" xml:id="C0009-2">
                    <l n="9" xml:id="C009-9"> Yo fas tot quant me diu lo <choice>
                            <abbr>pensamȇt</abbr>
                            <expan>pensament</expan>
                        </choice></l>
                    <l n="10" xml:id="C009-10"> E si agues tant seny com salamo</l>
                    <l n="11" xml:id="C009-11"> Fora tot poch en dar occasio</l>
                    <l n="12" xml:id="C009-12"> Que no temes ason gran <choice>
                            <abbr>manamȇt</abbr>
                            <expan>manament</expan>
                        </choice>t</l>
                    <l n="13" xml:id="C009-13"> Lo qui no sap <choice>
                            <abbr>ꝗ</abbr>
                            <expan>que</expan>
                        </choice> pot fer molt amar</l>
                    <l n="14" xml:id="C009-14"> Yoli perdo si de mj va trufan</l>
                    <l n="15" xml:id="C009-15"> Mas ligals fets quen veritat stan</l>
                    <l n="16" xml:id="C009-16"> E sabra molts de grat la mort passar</l>
                </lg>
                <lg type="cobla" n="3" xml:id="C0009-3">
                    <l n="17" xml:id="C009-17"> Si no es pech quis deu maravellar</l>
                    <l n="18" xml:id="C009-18"> Dalgun cas fort <choice>
                            <abbr>ꝗ</abbr>
                            <expan>que</expan>
                        </choice> segueixque laman</l>
                    <l n="19" xml:id="C009-19"> Lom fora seny no pot ser ben vsan</l>
                    <l n="20" xml:id="C009-20"> Tal me confes donchs nom vullau reptar</l>
                    <l n="21" xml:id="C009-21"> Lo carrech pren amor sin res fallesch</l>
                    <l n="22" xml:id="C009-22"> Car so abstret de seny e de saber</l>
                    <l n="23" xml:id="C009-23"> E res no fas encontra son voler</l>
                    <l n="24" xml:id="C009-24"> Desijant be la dolor li grahesch.</l>
                </lg>
                <pb n="10v"
                    facs="http://www.cervantesvirtual.com/obra-visor/obres-dausias-march-manuscrito--0/html/01f79830-82b2-11df-acc7-002185ce6064_24.html"/>
                <lg type="cobla" n="4" xml:id="C0009-4">
                    <l n="25" xml:id="C009-25"> Stant apart e yom empeguesch</l>
                    <l n="26" xml:id="C009-26">
                        <choice>
                            <abbr>Ymagint</abbr>
                            <expan>Ymaginant</expan>
                        </choice> com que deuria fer</l>
                    <l n="27" xml:id="C009-27"> Dexecutar no dech hauer sper</l>
                    <l n="28" xml:id="C009-28"> Puys lo primer assatx no enseguesch</l>
                    <l n="29" xml:id="C009-29"> Limaginar amor me vol <choice>
                            <abbr>repȇrre</abbr>
                            <expan>repenrre</expan>
                        </choice>rre</l>
                    <l n="30" xml:id="C009-30"> Tant largameent ab vergonyam refrena</l>
                    <l n="31" xml:id="C009-31"> Com se fara que ab cara serena</l>
                    <l n="32" xml:id="C009-32"> Aija poder de ma raho stenrre</l>
                </lg>
                <lg type="cobla" n="5" xml:id="C0009-5">
                    <l n="33" xml:id="C009-33"> Los fets damor yo no puch ben entendre</l>
                    <l n="34" xml:id="C009-34"> De grans contrasts mopinio es plena</l>
                    <l n="35" xml:id="C009-35"> Horan lo jorn que no sent vlla pena</l>
                    <l n="36" xml:id="C009-36"> Penssant en ço que vinch alarma rendre</l>
                    <l n="37" xml:id="C009-37"> Si altra veu limaginar mi porta</l>
                    <l n="38" xml:id="C009-38"> Per dar senyal <choice>
                            <abbr>ꝗ</abbr>
                            <expan>que</expan>
                        </choice> yo sia cregut</l>
                    <l n="39" xml:id="C009-39"> Suplich lamort quen tal cas maiut</l>
                    <l n="40" xml:id="C009-40"> E si nom val ma veritat iau morta</l>
                </lg>
                <lg type="tornada">
                    <l n="41" xml:id="C009-41"> Lir entre carts fins a veure la porta</l>
                    <l n="42" xml:id="C009-42"> De mos delits sobirans so vengut</l>
                    <l n="43" xml:id="C009-43"> Noy he tocat ans men torn com amut</l>
                    <l n="44" xml:id="C009-44"> E per tornar ja trop la vida torta</l>
                </lg>
            </div>
