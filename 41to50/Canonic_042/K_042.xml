 <text>
  <body>
   <div type="poem" xml:id="C042-K042">
    <pb n="40v"
     facs="http://www.cervantesvirtual.com/obra-visor/les-obres-de-mossen-ausias-march-sont-acabada-de-scriure-la-present-obra-per-m-pere-vilaraso-prevere-per-manament-de-lilltre-sor-almirant-de-napols-manuscrit--0/html/00488f44-82b2-11df-acc7-002185ce6064_87.html"/>
    <lg type="cobla" n="1" xml:id="C042-lg1">
     <l n="1" xml:id="C042-l1">v Os qui sabeu dela tortral costum </l>
     <l n="2" xml:id="C042-l2"> E si nou feu placial vos hoir </l>
     <l n="3" xml:id="C042-l3"> Quant mort li tol son par li vol jaquir </l>
     <l n="4" xml:id="C042-l4"> Dobres damor ne beu aygua de flum </l>
     <l n="5" xml:id="C042-l5"> Ans en los clots anssutsa primer laygua </l>
     <l n="6" xml:id="C042-l6"> Nes posa may en vert arbre fullat </l>
     <l n="7" xml:id="C042-l7"> Mas contra ço es <choice>
       <abbr>vrȃ</abbr>
       <expan>vostra</expan>
      </choice> qualitat </l>
     <l n="8" xml:id="C042-l8"> Per <choice>
       <abbr>grȃ</abbr>
       <expan>gran</expan>
      </choice> desitx no cast <choice>
       <abbr>quȇ</abbr>
       <expan>quen</expan>
      </choice> vos serayga </l>
    </lg>
    <pb n="41r"
     facs="http://www.cervantesvirtual.com/obra-visor/les-obres-de-mossen-ausias-march-sont-acabada-de-scriure-la-present-obra-per-m-pere-vilaraso-prevere-per-manament-de-lilltre-sor-almirant-de-napols-manuscrit--0/html/00488f44-82b2-11df-acc7-002185ce6064_88.html"/>
    <lg type="cobla" n="2" xml:id="C042-lg2">
     <l n="9" xml:id="C042-l9">
      <c rend="large">E</c> no cuydeu dona <choice>
       <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice> beus scayga </l>
     <l n="10" xml:id="C042-l10"> Que puys hagues tastat la carn <choice>
       <abbr>gȇtil</abbr>
       <expan>gentil</expan>
      </choice>
     </l>
     <l n="11" xml:id="C042-l11"> A mercader liuras vostre cos vil </l>
     <l n="12" xml:id="C042-l12"> E son dret nom en joan me pens cayga </l>
     <l n="13" xml:id="C042-l13"> E si voleu queus ne do conexença </l>
     <l n="14" xml:id="C042-l14"> Sa fas es gran ab la vista molt losca </l>
     <l n="15" xml:id="C042-l15"> Sos <choice>
       <abbr>fonamȇts</abbr>
       <expan>fonaments</expan>
      </choice> son <choice>
       <abbr>ƌ</abbr>
       <expan>de</expan>
      </choice> llagost o mosca </l>
     <l n="16" xml:id="C042-l16"> Cert no merex draps vendre de <choice>
       <abbr>florȇça</abbr>
       <expan>florença</expan>
      </choice>
     </l>
    </lg>
    <lg type="cobla" n="3" xml:id="C042-lg3">
     <l n="17" xml:id="C042-l17">
      <c rend="large">E</c> conexent la <choice>
       <abbr>vrȃ</abbr>
       <expan>vostra</expan>
      </choice> gran fallença </l>
     <l n="18" xml:id="C042-l18"> Volgues <choice>
       <abbr>mȗtar</abbr>
       <expan>muntar</expan>
      </choice> en amar caualler </l>
     <l n="19" xml:id="C042-l19"> E sabent ell tot <choice>
       <abbr>vrȇ</abbr>
       <expan>vostre</expan>
      </choice> fet en ver </l>
     <l n="20" xml:id="C042-l20"> En vos amar se tengra concientia </l>
     <l n="21" xml:id="C042-l21"> Sabent molt clar lansutziada vida </l>
     <l n="22" xml:id="C042-l22"> Prenent pubblich les pagues <choice>
       <abbr>ƌl</abbr>
       <expan>del</expan>
      </choice> peccat </l>
     <l n="23" xml:id="C042-l23"> Vostre cos letx per drap es baretat </l>
     <l n="24" xml:id="C042-l24"> Vostre serujr es bo sols per a dida </l>
    </lg>
    <lg type="cobla" n="4" xml:id="C042-lg4">
     <l n="25" xml:id="C042-l25">
      <c rend="large">E</c> no cuydeu fillaus agues jaquida </l>
     <l n="26" xml:id="C042-l26"> Vos alletant <choice>
       <abbr>aꝗllab</abbr>
       <expan>aquellab</expan>
      </choice> vostra llet </l>
     <l n="27" xml:id="C042-l27"> Car <choice>
       <abbr>vrȇ</abbr>
       <expan>vostre</expan>
      </choice> cos es de verj replet </l>
     <l n="28" xml:id="C042-l28"> E mostran ho <choice>
       <abbr>ʋres</abbr>
       <expan>vostres</expan>
      </choice> pels fora mjda </l>
     <l n="29" xml:id="C042-l29"> Car sius iaquiu vostra barba criada </l>
     <l n="30" xml:id="C042-l30"> E laus toleu puys ab los pels <choice>
       <abbr>dɫs</abbr>
       <expan>dels</expan>
      </choice> braços </l>
     <l n="31" xml:id="C042-l31"> Porança fer auantaiosos llacos </l>
     <l n="32" xml:id="C042-l32"> Prenjnts perdius tortra o cugullada </l>
    </lg>
    <lg type="cobla" n="5" xml:id="C042-lg5">
     <l n="33" xml:id="C042-l33">
      <c rend="large">Q</c><choice>
       <abbr>uȃt</abbr>
       <expan>uant</expan>
      </choice> hoire<add place="interlinear">u</add> alcauota prouada </l>
     <l n="34" xml:id="C042-l34"> Responeu tost <choice>
       <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice> que per vos ho <choice>
       <abbr>dirȃ</abbr>
       <expan>diran</expan>
      </choice>
     </l>
     <pb n="41v"
      facs="http://www.cervantesvirtual.com/obra-visor/les-obres-de-mossen-ausias-march-sont-acabada-de-scriure-la-present-obra-per-m-pere-vilaraso-prevere-per-manament-de-lilltre-sor-almirant-de-napols-manuscrit--0/html/00488f44-82b2-11df-acc7-002185ce6064_89.html"/>
     <l n="35" xml:id="C042-l35">E puys per nom propi vos <choice>
       <abbr>cridarȃ</abbr>
       <expan>cridaran</expan>
      </choice></l>
     <l n="36" xml:id="C042-l36"> Ja nous mostreu en loir enpatxada </l>
     <l n="37" xml:id="C042-l37"> Enterrogant amichs e <choice>
       <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice> voleu </l>
     <l n="38" xml:id="C042-l38"> Endret damor voleu res <choice>
       <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice> fer pusca </l>
     <l n="39" xml:id="C042-l39"> Tracte semblant yames me trobe cusca </l>
     <l n="40" xml:id="C042-l40"> Presta sere aquant demanareu </l>
    </lg>
    <lg type="tornada" n="6" xml:id="C042-lg6">
     <l n="41" xml:id="C042-l41"><c rend="large">T</c>ots los qui trob / o <choice>
       <abbr>cunsamȇt</abbr>
       <expan>cunsament</expan>
      </choice> volrreu </l>
     <l n="42" xml:id="C042-l42"> E en fets damor anprau na <choice>
       <abbr>mȏbohi</abbr>
       <expan>monbohi</expan>
      </choice>
     </l>
     <l n="43" xml:id="C042-l43"> Ellaus fara tot lo <choice>
       <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice> feu amj </l>
     <l n="44" xml:id="C042-l44"> Nos pot saber landres <choice>
       <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice> trobareu </l>
    </lg>
   </div>
  </body>
 </text>
