 <text>
  <body>
   <div type="poem" xml:id="C072-K072">

    <pb n="71r"
     facs="https://www.cervantesvirtual.com/obra-visor/les-obres-de-mossen-ausias-march-sont-acabada-de-scriure-la-present-obra-per-m-pere-vilaraso-prevere-per-manament-de-lilltre-sor-almirant-de-napols-manuscrit--0/html/00488f44-82b2-11df-acc7-002185ce6064_148.html"/>

    <lg type="cobla" n="1" xml:id="C072-lg1">

     <l n="1" xml:id="C072-l1"> p Aor <choice>
       <abbr>nō</abbr>
       <expan>non</expan>
      </choice> sent <choice>
       <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice> sobres laus me <choice>
       <abbr>vēça</abbr>
       <expan>vença</expan>
      </choice>
     </l>

     <l n="2" xml:id="C072-l2"> Loant <choice>
       <abbr>aꝗll</abbr>
       <expan>aquell</expan>
      </choice> q<del rend="illegible" extent="two characters"/> totes lengues loen </l>


     <l n="3" xml:id="C072-l3"> Guardant honor <choice>
       <abbr>aꝗll</abbr>
       <expan>aquell</expan>
      </choice> eternal esser </l>

     <l n="4" xml:id="C072-l4"> Hon tota res en ell es pus <choice>
       <abbr>ꝑfeta</abbr>
       <expan>perfeta</expan>
      </choice>
     </l>

     <l n="5" xml:id="C072-l5"> Quen si no es <choice>
       <abbr>obrāt</abbr>
       <expan>obrant</expan>
      </choice>
      <choice>
       <abbr>quāt</abbr>
       <expan>quant</expan>
      </choice> pot natura </l>

     <l n="6" xml:id="C072-l6"> Ans he pahor <choice>
       <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice>
      <choice>
       <abbr>mō</abbr>
       <expan>mon</expan>
      </choice> parlar no <choice>
       <abbr>cūpla</abbr>
       <expan>cumpla</expan>
      </choice>
     </l>

     <l n="7" xml:id="C072-l7"> En publicar part <choice>
       <abbr>đ</abbr>
       <expan>de</expan>
      </choice> sa justa fama </l>

     <l n="8" xml:id="C072-l8"> Tal com <choice>
       <abbr>reꝗr</abbr>
       <expan>requer</expan>
      </choice> ils merits seus latrassen </l>

    </lg>

    <lg type="cobla" n="2" xml:id="C072-lg2">

     <l n="9" xml:id="C072-l9"> <c rend="large">L</c>om anuios son offici reposa </l>

     <l n="10" xml:id="C072-l10"> Car degualtat ab ell ningu pareia </l>

     <l n="11" xml:id="C072-l11"> En ell penssant cascu si justifica </l>

     <l n="12" xml:id="C072-l12"> Tallant de si lamor assa <choice>
       <abbr>ꝑsona</abbr>
       <expan>persona</expan>
      </choice>
     </l>

     <l n="13" xml:id="C072-l13"> Tant son en ell las virtuts manifestes </l>

     <l n="14" xml:id="C072-l14"> Ques dira cech lom qui be no las veie </l>

     <l n="15" xml:id="C072-l15"> Per los mjtxs vaqi<choice>
       <abbr>ñ</abbr>
       <expan>en</expan>
      </choice><note type="editorial"> Per va qui en.</note> los strems no toqua</l>
     <l n="16" xml:id="C072-l16"> En <choice>
       <abbr>tēps</abbr>
       <expan>temps</expan>
      </choice> dels deus en vida ladoraren </l>

    </lg>

    <lg type="cobla" n="3" xml:id="C072-lg3">

     <l n="17" xml:id="C072-l17"> <c rend="large">E</c> deus vehent la perlongada honta </l>

     <l n="18" xml:id="C072-l18"> Quels <choice>
       <abbr>grās</abbr>
       <expan>grans</expan>
      </choice> senyos <choice>
       <abbr>encōtra</abbr>
       <expan>encontra</expan>
      </choice> dell <choice>
       <abbr>coḿten</abbr>
       <expan>cometen</expan>
      </choice>
     </l>

     <l n="19" xml:id="C072-l19"> Tenjnt ab fraus e tirannes maneres </l>

     <l n="20" xml:id="C072-l20"> Les <choice>
       <abbr>dɫ</abbr>
       <expan>del</expan>
      </choice> mon los pochs e <choice>
       <abbr>grās</abbr>
       <expan>grans</expan>
      </choice> realmes </l>

     <l n="21" xml:id="C072-l21"> Hadat voler al justificat home </l>

     <pb n="71v"
      facs="https://www.cervantesvirtual.com/obra-visor/les-obres-de-mossen-ausias-march-sont-acabada-de-scriure-la-present-obra-per-m-pere-vilaraso-prevere-per-manament-de-lilltre-sor-almirant-de-napols-manuscrit--0/html/00488f44-82b2-11df-acc7-002185ce6064_149.html"/>

     <l n="22" xml:id="C072-l22"> Quen breu spay haia la monarchia </l>

     <l n="23" xml:id="C072-l23"> Clar la nomen ab <choice>
      <abbr>aꝗstaltran</abbr>
      <expan>aquestaltran</expan>
     </choice> senya </l>

     <l n="24" xml:id="C072-l24"> Com <choice>
       <abbr>đ</abbr>
       <expan>de</expan>
      </choice> tot cert es dels <choice>
       <abbr>homēs</abbr>
       <expan>homens</expan>
      </choice> pus sauj </l>

    </lg>

    <lg type="cobla" n="4" xml:id="C072-lg4">

     <l n="25" xml:id="C072-l25"> <c rend="large">P</c>er ço <choice>
       <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice> mjls atotes gens se mostra </l>

     <l n="26" xml:id="C072-l26">
      <choice>
       <abbr>Mostrātlaꝗlls</abbr>
       <expan>Mostrantlaquells</expan>
      </choice> quj <choice>
       <abbr>vaꝗs</abbr>
       <expan>vaques</expan>
      </choice> e bochs <choice>
       <abbr>guardē</abbr>
       <expan>guarden</expan>
      </choice>
     </l>

     <l n="27" xml:id="C072-l27"> Ell es <choice>
       <abbr>aꝗll</abbr>
       <expan>aquell</expan>
      </choice> quin sa jouentut tendra </l>

     <l n="28" xml:id="C072-l28">
      <choice>
       <abbr>Sobrā</abbr>
       <expan>Sobran</expan>
      </choice>
      <choice>
       <abbr>aꝗlla</abbr>
       <expan>aquella</expan>
      </choice> qui sauiesa colen </l>

     <l n="29" xml:id="C072-l29"> E despocat <choice>
       <abbr>đ</abbr>
       <expan>de</expan>
      </choice>
      <choice>
       <abbr>nōbre</abbr>
       <expan>nombre</expan>
      </choice> de gent darmes </l>

     <l n="30" xml:id="C072-l30"> Les multituts sens colp ha fetes retre </l>

     <l n="31" xml:id="C072-l31"> Tot quant pot fer virtut <choice>
       <abbr>đ</abbr>
       <expan>de</expan>
      </choice> fortalesa </l>

     <l n="32" xml:id="C072-l32"> En vn sols hom en <choice>
       <abbr>aꝗst</abbr>
       <expan>aquest</expan>
      </choice> ho demostra </l>

    </lg>

    <lg type="cobla" n="5" xml:id="C072-lg5">

     <l n="33" xml:id="C072-l33"> <c rend="large">E</c>n gran defalt es lo <choice>
       <abbr>mō</abbr>
       <expan>mon</expan>
      </choice> de poetes </l>

     <l n="34" xml:id="C072-l34"> Per enbellir los fets de qui be obren </l>

     <l n="35" xml:id="C072-l35"> Nos <choice>
       <abbr>freturāts</abbr>
       <expan>freturants</expan>
      </choice> de bella <choice>
       <abbr>eloꝗnçia</abbr>
       <expan>eloquençia</expan>
      </choice>
     </l>

     <l n="36" xml:id="C072-l36"> Lorella dom affalach no pot rebre </l>

     <l n="37" xml:id="C072-l37">
      <choice>
       <abbr>Daꝗst</abbr>
       <expan>Daquest</expan>
      </choice> valent vna gran <choice>
       <abbr>trōpa</abbr>
       <expan>trompa</expan>
      </choice> sona </l>

     <l n="38" xml:id="C072-l38"> Quels <choice>
       <abbr>jndiās</abbr>
       <expan>jndians</expan>
      </choice> ab vn poch no axorda </l>

     <l n="39" xml:id="C072-l39">
      <choice>
       <abbr>Houē</abbr>
       <expan>Houen</expan>
      </choice> la<choice>
       <abbr>ꝗlls</abbr>
       <expan>quells</expan>
      </choice> qui son <choice>
       <abbr>atremūtana</abbr>
       <expan>atremuntana</expan>
      </choice>
     </l>

     <l n="40" xml:id="C072-l40"> Yls de ponent y de <choice>
       <abbr>lleuāt</abbr>
       <expan>lleuant</expan>
      </choice> los pobles </l>

    </lg>

    <lg type="tornada" n="6" xml:id="C072-lg6">

     <l n="41" xml:id="C072-l41"> f Oll es <choice>
       <abbr>aꝗll</abbr>
       <expan>aquell</expan>
      </choice> qui fa juy en los <choice>
       <abbr>homēs</abbr>
       <expan>homens</expan>
      </choice>
     </l>

     <l n="42" xml:id="C072-l42"> Segons <choice>
       <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice> dells la fortuna ordena </l>

     <l n="43" xml:id="C072-l43">
      <choice>
       <abbr>Aꝗlls</abbr>
       <expan>Aquells</expan>
      </choice> affes qui no son en larbitre </l>

     <l n="44" xml:id="C072-l44"> Colpa noy cau si <choice>
       <abbr>venē</abbr>
       <expan>venen</expan>
      </choice> per <choice>
       <abbr>cōtrarj</abbr>
       <expan>contrarj</expan>
      </choice>
     </l>

    </lg>

   </div>
  </body>
 </text>
