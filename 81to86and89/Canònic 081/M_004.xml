   <text>
      <body>
         <div type="poem" xml:id="C081-M004">

            <pb n="147r"
               facs="https://www.cervantesvirtual.com/obra-visor/cancionero-catalan-del-ateneu-ba1-manuscrit--0/html/01e69dd2-82b2-11df-acc7-002185ce6064_297.html"/>

            <lg type="cobla" n="1" xml:id="C081-lg1">

               <l n="1" xml:id="C081-l1"> Axi com sell / quis ueu prop dela mort </l>

               <l n="2" xml:id="C081-l2"> Corent mal temps / parilant enlamar </l>

               <l n="3" xml:id="C081-l3"> Eueu loloch / honsepot restaurar </l>

               <l n="4" xml:id="C081-l4"> Enoyateyn / <choice>
                     <abbr>ꝑ</abbr>
                     <expan>per</expan>
                  </choice> sa maluada sort </l>

               <l n="5" xml:id="C081-l5"> Ne pren amj / queuaig afayns pasant </l>

               <l n="6" xml:id="C081-l6"> E ueig auos / bastant mosmals djljr </l>

               <l n="7" xml:id="C081-l7"> Desesperat / demos dasigs compljr </l>

               <l n="8" xml:id="C081-l8"> Irepel mon / vostrergul racitant </l>

            </lg>

         </div>
      </body>
