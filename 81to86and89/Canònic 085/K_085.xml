 <text>
  <body>
   <div type="poem" xml:id="C085-K085">

    <pb n="82r"
     facs="https://www.cervantesvirtual.com/obra-visor/les-obres-de-mossen-ausias-march-sont-acabada-de-scriure-la-present-obra-per-m-pere-vilaraso-prevere-per-manament-de-lilltre-sor-almirant-de-napols-manuscrit--0/html/00488f44-82b2-11df-acc7-002185ce6064_170.html"/>

    <lg type="cobla" n="1" xml:id="C085-lg1">

     <l n="1" xml:id="C085-l1"> l Exe la sort lo seu variat torn </l>

     <l n="2" xml:id="C085-l2"> Cesse amor son doloros costum </l>

     <l n="3" xml:id="C085-l3"> Dell so content si be nou acostum </l>

     <l n="4" xml:id="C085-l4"> Nom pot donar mes en lo <choice>
       <abbr>ṕt</abbr>
       <expan>present</expan>
      </choice> jorn </l>

     <l n="5" xml:id="C085-l5"> Mas jo vull ço <choice>
       <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice> natura no te </l>

     <l n="6" xml:id="C085-l6"> E desitx mes <choice>
       <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice> jo no puch trobar </l>

     <l n="7" xml:id="C085-l7"> Volent <choice>
       <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice> res no pogues enpatxar </l>

     <l n="8" xml:id="C085-l8">
      <choice>
       <abbr>Aꝗll</abbr>
       <expan>Aquell</expan>
      </choice> delit <choice>
       <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice>
      <choice>
       <abbr>ꝑ</abbr>
       <expan>per</expan>
      </choice> amor me ve </l>

    </lg>

    <lg type="cobla" n="2" xml:id="C085-lg2">

     <l n="9" xml:id="C085-l9"> Ara conech <choice>
       <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice> preu mes <choice>
       <abbr>aꝗst</abbr>
       <expan>aquest</expan>
      </choice> be </l>

     <l n="10" xml:id="C085-l10"> Que tots <choice>
       <abbr>aꝗlls</abbr>
       <expan>aquells</expan>
      </choice>
      <choice>
       <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice> jo poriauer </l>

     <l n="11" xml:id="C085-l11"> E <choice>
       <abbr>đls</abbr>
       <expan>dels</expan>
      </choice> passats <choice>
       <abbr>cōpte</abbr>
       <expan>compte</expan>
      </choice> no <choice>
       <abbr>mē</abbr>
       <expan>men</expan>
      </choice> cal fer </l>

     <l n="12" xml:id="C085-l12"> Foren no res segon se veura be </l>

     <l n="13" xml:id="C085-l13"> Delit damor no prehi <choice>
       <abbr>yaḿs</abbr>
       <expan>yames</expan>
      </choice> tant </l>

     <l n="14" xml:id="C085-l14"> Que fos çelos <choice>
       <abbr>đ</abbr>
       <expan>de</expan>
      </choice> tot lauenjdor </l>

     <l n="15" xml:id="C085-l15"> Mas de present tem <choice>
       <abbr>quē</abbr>
       <expan>quem</expan>
      </choice> falegamor </l>

     <l n="16" xml:id="C085-l16"> E <choice>
       <abbr>quāt</abbr>
       <expan>quant</expan>
      </choice> me pot noure yo so <choice>
       <abbr>dubtāt</abbr>
       <expan>dubtant</expan>
      </choice>
     </l>

    </lg>

    <lg type="cobla" n="3" xml:id="C085-lg3">

     <l n="17" xml:id="C085-l17"> E si demj jorn trop segur penssant </l>

     <l n="18" xml:id="C085-l18"> A uos yo tem aytant <choice>
       <abbr>cō</abbr>
       <expan>com</expan>
      </choice> enemjch </l>

     <l n="19" xml:id="C085-l19"> Mas sobre tot amor tem <choice>
       <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice> desllich </l>

     <l n="20" xml:id="C085-l20"> Lo <choice>
       <abbr>ligamēt</abbr>
       <expan>ligament</expan>
      </choice> del qual ell fonch <choice>
       <abbr>ligāt</abbr>
       <expan>ligant</expan>
      </choice>
     </l>

     <l n="21" xml:id="C085-l21">
      <choice>
       <abbr>Aꝗll</abbr>
       <expan>Aquell</expan>
      </choice> voler <choice>
       <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice> yo puch be regir </l>

     <l n="22" xml:id="C085-l22">
      <choice>
       <abbr>Nō</abbr>
       <expan>Nom</expan>
      </choice> fa pahor ne res <choice>
       <abbr>ꝗn</abbr>
       <expan>quen</expan>
      </choice> mj sera </l>

     <l n="23" xml:id="C085-l23">
      <choice>
       <abbr>Aꝗll</abbr>
       <expan>Aquell</expan>
      </choice> voler <choice>
       <abbr>quē</abbr>
       <expan>quen</expan>
      </choice> patio stara </l>

     <l n="24" xml:id="C085-l24">
      <choice>
       <abbr>Donē</abbr>
       <expan>Donem</expan>
      </choice> la mort <choice>
       <abbr>quāt</abbr>
       <expan>quant</expan>
      </choice> me volrra jaquir </l>

    </lg>

    <pb n="82v"
     facs="https://www.cervantesvirtual.com/obra-visor/les-obres-de-mossen-ausias-march-sont-acabada-de-scriure-la-present-obra-per-m-pere-vilaraso-prevere-per-manament-de-lilltre-sor-almirant-de-napols-manuscrit--0/html/00488f44-82b2-11df-acc7-002185ce6064_171.html"/>

    <lg type="cobla" n="4" xml:id="C085-lg4">

     <l n="25" xml:id="C085-l25"> Per <choice>
       <abbr>ꝗs</abbr>
       <expan>ques</expan>
      </choice> pot fer <choice>
       <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice> pot dimjnuyr </l>

     <l n="26" xml:id="C085-l26"> En mj amor sens <choice>
       <abbr>mō</abbr>
       <expan>mon</expan>
      </choice>
      <choice>
       <abbr>consentjmēt</abbr>
       <expan>consentjment</expan>
      </choice>
     </l>

     <l n="27" xml:id="C085-l27"> Ja sent dolor e <choice>
       <abbr>cō</abbr>
       <expan>com</expan>
      </choice>
      <choice>
       <abbr>mō</abbr>
       <expan>mon</expan>
      </choice> sentiment </l>

     <l n="28" xml:id="C085-l28"> No es forçat <choice>
       <abbr>đ</abbr>
       <expan>de</expan>
      </choice>
      <choice>
       <abbr>mōn</abbr>
       <expan>mon</expan>
      </choice> voler seguir </l>

     <l n="29" xml:id="C085-l29"> E yo segur damor e mi e vos </l>

     <l n="30" xml:id="C085-l30">
      <choice>
       <abbr>Tendrē</abbr>
       <expan>Tendrem</expan>
      </choice> per cert desser benuyrat </l>

     <l n="31" xml:id="C085-l31"> Als no mj fall sino seguretat </l>

     <l n="32" xml:id="C085-l32"> Car en present bast esser glorios </l>

    </lg>

    <lg type="cobla" n="5" xml:id="C085-lg5">

     <l n="33" xml:id="C085-l33"> James amj que no fos glorios </l>

     <l n="34" xml:id="C085-l34"> Daquell desitx <choice>
       <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice> per freturauem </l>

     <l n="35" xml:id="C085-l35">
      <choice>
       <abbr>Dalgūa</abbr>
       <expan>Dalguna</expan>
      </choice> part lo meu voler fon sem </l>

     <l n="36" xml:id="C085-l36"> Per <choice>
       <abbr>quē</abbr>
       <expan>quem</expan>
      </choice> falli en part ser delitos </l>

     <l n="37" xml:id="C085-l37"> Amor amj <choice>
       <abbr>strāys</abbr>
       <expan>stranys</expan>
      </choice>
      <choice>
       <abbr>termēs</abbr>
       <expan>termens</expan>
      </choice> ha mes </l>

     <l n="38" xml:id="C085-l38">
      <choice>
       <abbr>Mō</abbr>
       <expan>Mon</expan>
      </choice> desitx es <choice>
       <abbr>cōplit</abbr>
       <expan>complit</expan>
      </choice>
      <choice>
       <abbr>ꝑ</abbr>
       <expan>per</expan>
      </choice> vos de tot </l>

     <l n="39" xml:id="C085-l39"> A fermetat vull sol hauer vn mot </l>

     <l n="40" xml:id="C085-l40"> Que denemjchs damor sia defes </l>

    </lg>

    <lg type="cobla" n="6" xml:id="C085-lg6">

     <l n="41" xml:id="C085-l41"> Vostre voler ma en amor empes </l>

     <l n="42" xml:id="C085-l42"> E lo voler <choice>
       <abbr>quē</abbr>
       <expan>quem</expan>
      </choice> sembla ser tot meu </l>

     <l n="43" xml:id="C085-l43"> E lalt secret <choice>
       <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice> fer <choice>
       <abbr>cōte</abbr>
       <expan>conte</expan>
      </choice> nos deu </l>

     <l n="44" xml:id="C085-l44"> Car forcat fuy <choice>
       <abbr>daꝗst</abbr>
       <expan>daquest</expan>
      </choice> loch ser ances </l>

     <l n="45" xml:id="C085-l45"> Si la valor <choice>
       <abbr>ʋre</abbr>
       <expan>vostre</expan>
      </choice> y el voler fall </l>

     <l n="46" xml:id="C085-l46"> Ab ells mesclat lo meu alt finira </l>

     <l n="47" xml:id="C085-l47"> Mentres vullau e valgau no morra </l>

     <l n="48" xml:id="C085-l48">
      <choice>
       <abbr>Daꝗstes</abbr>
       <expan>Daquestes</expan>
      </choice> tres pedres fas <choice>
       <abbr>mō</abbr>
       <expan>mon</expan>
      </choice> fermall </l>

    </lg>

    <lg type="cobla" n="7" xml:id="C085-lg7">

     <l n="49" xml:id="C085-l49"> Tot scriuent jutja lo seu treball </l>

     <l n="50" xml:id="C085-l50"> Pus affanyos <choice>
       <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice> lo del cauador </l>

     <pb n="83r"
      facs="https://www.cervantesvirtual.com/obra-visor/les-obres-de-mossen-ausias-march-sont-acabada-de-scriure-la-present-obra-per-m-pere-vilaraso-prevere-per-manament-de-lilltre-sor-almirant-de-napols-manuscrit--0/html/00488f44-82b2-11df-acc7-002185ce6064_172.html"/>

     <l n="51" xml:id="C085-l51"> Tot axin pren a <choice>
       <abbr>cascū</abbr>
       <expan>cascun</expan>
      </choice> amador </l>

     <l n="52" xml:id="C085-l52"> Baxant tot cas y ell sen <choice>
       <abbr>mūta</abbr>
       <expan>munta</expan>
      </choice> cauall </l>

     <l n="53" xml:id="C085-l53"> Y el pobra hom fa juy del tirant </l>

     <l n="54" xml:id="C085-l54"> Que son pensar <choice>
       <abbr>tosŧps</abbr>
       <expan>tostemps</expan>
      </choice> corra delit </l>

     <l n="55" xml:id="C085-l55"> De deu es ja qui viu ab null despit </l>

     <l n="56" xml:id="C085-l56"> Car lo pus rich del <choice>
       <abbr>mō</abbr>
       <expan>mon</expan>
      </choice> es <choice>
       <abbr>probreiāt</abbr>
       <expan>probreiant</expan>
      </choice>
     </l>

    </lg>

    <lg type="cobla" n="8" xml:id="C085-lg8">

     <l n="57" xml:id="C085-l57"> Yo so amat e visch doloreiant </l>

     <l n="58" xml:id="C085-l58"> Daltres dolos quel no amat sab </l>

     <l n="59" xml:id="C085-l59"> Yo tem de mort lo cor e mes lo cap </l>

     <l n="60" xml:id="C085-l60">
      <choice>
       <abbr>Daꝗlla</abbr>
       <expan>Daquella</expan>
      </choice>
      <choice>
       <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice> de present es <choice>
       <abbr>amāt</abbr>
       <expan>amant</expan>
      </choice>
     </l>

     <l n="61" xml:id="C085-l61"> Yo molt mes am per serne <choice>
       <abbr>bēuolgut</abbr>
       <expan>benuolgut</expan>
      </choice>
     </l>

     <l n="62" xml:id="C085-l62"> Car <choice>
       <abbr>mō</abbr>
       <expan>mon</expan>
      </choice> delit desamor se nodrex </l>

     <l n="63" xml:id="C085-l63"> Si contramor lo seu fallex </l>

     <l n="64" xml:id="C085-l64"> Qui pensara lo mal amj <choice>
       <abbr>vēgut</abbr>
       <expan>vengut</expan>
      </choice>
     </l>

    </lg>

    <lg type="tornada" n="9" xml:id="C085-lg9">

     <l n="65" xml:id="C085-l65"> O ffollamor en dolor so caygut </l>

     <l n="66" xml:id="C085-l66"> Com no vetx hom <choice>
       <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice>parle be <choice>
       <abbr>đ</abbr>
       <expan>de</expan>
      </choice>vos </l>

     <l n="67" xml:id="C085-l67"> E vetx me tals <choice>
       <abbr>hauēts</abbr>
       <expan>hauents</expan>
      </choice> justes clamos </l>

     <l n="68" xml:id="C085-l68"> Deu guart amj desser en tal vengut </l>

    </lg>

   </div>
  </body>
 </text>
