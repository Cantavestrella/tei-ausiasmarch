 <text>
  <body>
   <div type="poem" xml:id="C089-K088">

    <pb n="91v"
     facs="https://www.cervantesvirtual.com/obra-visor/les-obres-de-mossen-ausias-march-sont-acabada-de-scriure-la-present-obra-per-m-pere-vilaraso-prevere-per-manament-de-lilltre-sor-almirant-de-napols-manuscrit--0/html/00488f44-82b2-11df-acc7-002185ce6064_189.html"/>

    <lg type="cobla" n="1" xml:id="C089-lg1">

     <l n="1" xml:id="C089-l1"> c Eruo ferit no desija la font </l>

     <l n="2" xml:id="C089-l2"> Aytant <choice>
       <abbr>cō</abbr>
       <expan>com</expan>
      </choice> yo esser a vos <choice>
       <abbr>presēt</abbr>
       <expan>present</expan>
      </choice>
     </l>

     <l n="3" xml:id="C089-l3"> Al <choice>
       <abbr>grā</abbr>
       <expan>gran</expan>
      </choice> repos <choice>
       <abbr>mō</abbr>
       <expan>mon</expan>
      </choice>
      <choice>
       <abbr>cōtentament</abbr>
       <expan>contentament</expan>
      </choice>
     </l>

     <l n="4" xml:id="C089-l4"> Passar no puch sino <choice>
       <abbr>ꝑ</abbr>
       <expan>per</expan>
      </choice>
      <choice>
       <abbr>aꝗst</abbr>
       <expan>aquest</expan>
      </choice> pont </l>

     <l n="5" xml:id="C089-l5"> Molt me ve tart lo jorn <choice>
       <abbr>tā</abbr>
       <expan>tan</expan>
      </choice> desijat </l>

     <l n="6" xml:id="C089-l6"> Comprat molt car per doloros sospir </l>

     <l n="7" xml:id="C089-l7"> E tart o breu so cert <choice>
       <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice> deu venjr </l>

     <l n="8" xml:id="C089-l8"> Si per la mort camj no mes tancat </l>

    </lg>

    <lg type="cobla" n="2" xml:id="C089-lg2">

     <l n="9" xml:id="C089-l9"> Esser no pusch <choice>
       <abbr>desꝑāça</abbr>
       <expan>desperança</expan>
      </choice>
      <choice>
       <abbr>lāçat</abbr>
       <expan>lançat</expan>
      </choice>
     </l>

     <l n="10" xml:id="C089-l10"> Car jous desitx segons <choice>
       <abbr>mō</abbr>
       <expan>mon</expan>
      </choice> major be </l>

     <l n="11" xml:id="C089-l11"> A vos <choice>
       <abbr>demā</abbr>
       <expan>deman</expan>
      </choice>
      <choice>
       <abbr>cōtra</abbr>
       <expan>contra</expan>
      </choice> mj res nous te </l>

     <l n="12" xml:id="C089-l12"> Mentrel voler <choice>
       <abbr>vostrē</abbr>
       <expan>vostren</expan>
      </choice> sia donat </l>

     <l n="13" xml:id="C089-l13"> Sil <choice>
       <abbr>pensamēt</abbr>
       <expan>pensament</expan>
      </choice> lunyaua vn sol <choice>
       <abbr>pūt</abbr>
       <expan>punt</expan>
      </choice>
     </l>

     <l n="14" xml:id="C089-l14"> Dimaginar hauer <choice>
       <abbr>ʋre</abbr>
       <expan>vostre</expan>
      </choice> voler </l>

     <l n="15" xml:id="C089-l15"> Sens <choice>
       <abbr>aꝗll</abbr>
       <expan>aquell</expan>
      </choice> tot no puch delithauer </l>

     <l n="16" xml:id="C089-l16"> Sinos tot sa tost pora ser defunt </l>

    </lg>

    <lg type="cobla" n="3" xml:id="C089-lg3">

     <l n="17" xml:id="C089-l17">
      <choice>
       <abbr>Dauāt</abbr>
       <expan>Dauant</expan>
      </choice> me vetx de <choice>
       <abbr>grās</abbr>
       <expan>grans</expan>
      </choice> dolos vn <choice>
       <abbr>mūt</abbr>
       <expan>munt</expan>
      </choice>
     </l>

     <l n="18" xml:id="C089-l18"> Puys ops e tant per almeu <choice>
       <abbr>cōtentar</abbr>
       <expan>contentar</expan>
      </choice>
     </l>

     <pb n="92r"
      facs="https://www.cervantesvirtual.com/obra-visor/les-obres-de-mossen-ausias-march-sont-acabada-de-scriure-la-present-obra-per-m-pere-vilaraso-prevere-per-manament-de-lilltre-sor-almirant-de-napols-manuscrit--0/html/00488f44-82b2-11df-acc7-002185ce6064_190.html"/>

     <l n="19" xml:id="C089-l19"> E mon voler pora molt menys cabar </l>

     <l n="20" xml:id="C089-l20"> Sil vostres mou e no mostra <choice>
       <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice>
      <choice>
       <abbr>mūt</abbr>
       <expan>munt</expan>
      </choice>
     </l>

     <l n="21" xml:id="C089-l21"> E deuallant deuallara lo meu </l>

     <l n="22" xml:id="C089-l22"> E dalt cahent no dara poch <choice>
       <abbr>ꝗbrant</abbr>
       <expan>quebrant</expan>
      </choice>
     </l>

     <l n="23" xml:id="C089-l23"> Car tot strem al <choice>
       <abbr>strē</abbr>
       <expan>strem</expan>
      </choice> es <choice>
       <abbr>donāt</abbr>
       <expan>donant</expan>
      </choice>
     </l>

     <l n="24" xml:id="C089-l24"> A poch stat no par lofensa greu </l>

    </lg>

    <lg type="cobla" n="4" xml:id="C089-lg4">

     <l n="25" xml:id="C089-l25"> Mil veus lo jorn es permj pregat deu </l>

     <l n="26" xml:id="C089-l26"> Daço <choice>
       <abbr>quē</abbr>
       <expan>quen</expan>
      </choice> vos esta la major part </l>

     <l n="27" xml:id="C089-l27">
      <choice>
       <abbr>Quē</abbr>
       <expan>Quen</expan>
      </choice> mon voler haiau lo vostresguart </l>

     <l n="28" xml:id="C089-l28"> E prech amor vos lanç tot poder seu </l>

     <l n="29" xml:id="C089-l29"> Si tal cas ve ladonchs pendreu <choice>
       <abbr>strē</abbr>
       <expan>strem</expan>
      </choice>
     </l>

     <l n="30" xml:id="C089-l30"> Si troba loch hon se <choice>
       <abbr>prēga</abbr>
       <expan>prenga</expan>
      </choice> en vos </l>

     <l n="31" xml:id="C089-l31"> Ab loch dispost sa patios en nos </l>

     <l n="32" xml:id="C089-l32"> E lo <choice>
       <abbr>cōtrast</abbr>
       <expan>contrast</expan>
      </choice>
      <choice>
       <abbr>tenɉ</abbr>
       <expan>tenjm</expan>
      </choice> e no volem </l>

    </lg>

    <lg type="cobla" n="5" xml:id="C089-lg5">

     <l n="33" xml:id="C089-l33"> Noues de vos saber <choice>
       <abbr>mortalmēt</abbr>
       <expan>mortalment</expan>
      </choice> tem </l>

     <l n="34" xml:id="C089-l34">
      <choice>
       <abbr>Dubtāt</abbr>
       <expan>Dubtant</expan>
      </choice> me fort <choice>
       <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice> noy mostreu amor </l>

     <l n="35" xml:id="C089-l35"> Per no saber visch en altra dolor </l>

     <l n="36" xml:id="C089-l36"> No se de qual costat guart <choice>
       <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice>
      <choice>
       <abbr>nō</abbr>
       <expan>nom</expan>
      </choice> crem </l>

     <l n="37" xml:id="C089-l37"> No es en vos <choice>
       <abbr>cōplir</abbr>
       <expan>complir</expan>
      </choice> lo meu desitx </l>

     <l n="38" xml:id="C089-l38"> Per be <choice>
       <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice> vos vullau <choice>
       <abbr>cōplir</abbr>
       <expan>complir</expan>
      </choice>
      <choice>
       <abbr>aꝗll</abbr>
       <expan>aquell</expan>
      </choice>
     </l>

     <l n="39" xml:id="C089-l39"> Damor haueu hauer forçat <choice>
       <abbr>cōçell</abbr>
       <expan>conçell</expan>
      </choice>
     </l>

     <l n="40" xml:id="C089-l40"> En vos y en ell recau <choice>
       <abbr>mō</abbr>
       <expan>mon</expan>
      </choice> be <choice>
       <abbr>cōplit</abbr>
       <expan>complit</expan>
      </choice>
     </l>

    </lg>

    <lg type="cobla" n="6" xml:id="C089-lg6">

     <l n="41" xml:id="C089-l41"> Res no temau ne <choice>
       <abbr>prēgau</abbr>
       <expan>prengau</expan>
      </choice> en despit </l>

     <l n="42" xml:id="C089-l42"> Dels <choice>
       <abbr>pensamēts</abbr>
       <expan>pensaments</expan>
      </choice> meus ab veritat </l>

     <l n="43" xml:id="C089-l43"> Car en seruey <choice>
       <abbr>serā</abbr>
       <expan>seran</expan>
      </choice> de fermetat </l>

     <l n="44" xml:id="C089-l44"> De tals <choice>
       <abbr>seruēts</abbr>
       <expan>seruents</expan>
      </choice> vol ser amor seruit </l>

     <pb n="92v"
      facs="https://www.cervantesvirtual.com/obra-visor/les-obres-de-mossen-ausias-march-sont-acabada-de-scriure-la-present-obra-per-m-pere-vilaraso-prevere-per-manament-de-lilltre-sor-almirant-de-napols-manuscrit--0/html/00488f44-82b2-11df-acc7-002185ce6064_191.html"/>

     <l n="45" xml:id="C089-l45"> Si <choice>
       <abbr>pūt</abbr>
       <expan>punt</expan>
      </choice> danutx dest practicar sentiu </l>

     <l n="46" xml:id="C089-l46"> Sens amor sou /o no sabeu <choice>
       <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice> vol </l>

     <l n="47" xml:id="C089-l47"> Ferm loch nol te qui <choice>
       <abbr>daꝗst</abbr>
       <expan>daquest</expan>
      </choice>
      <note type="editorial">Per daquest</note> mal sedol </l>

     <l n="48" xml:id="C089-l48"> Lo mouiment per serguretat tenju </l>

    </lg>

    <lg type="cobla" n="7" xml:id="C089-lg7">

     <l n="49" xml:id="C089-l49"> Si tant de vos <choice>
       <abbr>cō</abbr>
       <expan>com</expan>
      </choice> voleu no <choice>
       <abbr>cōfiu</abbr>
       <expan>confiu</expan>
      </choice>
     </l>

     <l n="50" xml:id="C089-l50">
      Mon<choice>
       <abbr>grā</abbr>
       <expan>gran</expan>
      </choice> voler me portan<choice>
       <abbr>aꝗst</abbr>
       <expan>aquest</expan>
      </choice> zel </l>

     <l n="51" xml:id="C089-l51"> De vostre cos no tem lo pus primpel </l>

     <l n="52" xml:id="C089-l52"> Quen contramj res fes nem fos altiu </l>

     <l n="53" xml:id="C089-l53"> La <choice>
       <abbr>volūtat</abbr>
       <expan>voluntat</expan>
      </choice> vull <choice>
       <abbr>ꝗ</abbr>
       <expan>que</expan>
      </choice> pas <choice>
       <abbr>totā</abbr>
       <expan>totan</expan>
      </choice> mj </l>

     <l n="54" xml:id="C089-l54"> Yo so zelos simolt amau adeu </l>

     <l n="55" xml:id="C089-l55"> Dant vos delit sens mj lomal crex <choice>
       <abbr>ḿu</abbr>
       <expan>meu</expan>
      </choice>
     </l>

     <l n="56" xml:id="C089-l56"> Quant vos dolgues <choice>
       <abbr>đl</abbr>
       <expan>del</expan>
      </choice> mal <choice>
       <abbr>vostrē</abbr>
       <expan>vostren</expan>
      </choice> dolguj </l>

    </lg>

    <lg type="tornada" n="8" xml:id="C089-lg8">

     <l n="57" xml:id="C089-l57"> Mon darrer be <choice>
       <abbr>đ</abbr>
       <expan>de</expan>
      </choice> vos yo guart la fi </l>

     <l n="58" xml:id="C089-l58"> Quant del present <choice>
       <abbr>ḿ</abbr>
       <expan>me</expan>
      </choice> trop esser <choice>
       <abbr>cōtent</abbr>
       <expan>content</expan>
      </choice>
     </l>

     <l n="59" xml:id="C089-l59"> E sim vetx trist per <choice>
       <abbr>algū</abbr>
       <expan>algun</expan>
      </choice> cas present </l>

     <l n="60" xml:id="C089-l60"> Res venidor trobar nos pot en mj </l>

    </lg>

   </div>
  </body>
 </text>
